 "html": "<div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <h3><b>\n                    <center><u> SUMMARY OF CORE TEST RESULTS </u> </center>\n                </b></h3><br>\n        </div>\n        \n        <div class=\"row\">\n        <div class=\"col-xs-2\">\n           <div align='right'> <b>Client</b></div>\n            </div>\n            <div class=\"col-xs-4\">\n            <div align='left'>:{{doc.customer or ''}}</div>\n            </div>\n               <div class=\"col-xs-3\">\n                  <div align='right'>   <b>Test Date</b></div>\n                </div>\n                <div class=\"col-xs-3\">\n                    :{{frappe.utils.formatdate(doc.test_date) or ''}}<br>\n                </div>\n                </div>\n              <div class=\"row\"> \n               <div class=\"col-xs-2\">\n            <div align='right'><b>Project Name</b></div>\n            </div>\n               <div class=\"col-xs-4\">\n                  <div align='left'> :{{doc.project_name or ''}}</div>\n                   </div>\n                    <div class=\"col-xs-3\">   \n                   <div align='right'> <b>Report Date</b></div>\n                    </div>\n                    <div class=\"col-xs-3\">   \n                    :{{frappe.utils.formatdate(doc.report_date) or ''}}<br>\n                  </div>\n                  </div> \n                       <div class=\"row\"> \n                     <div class=\"col-xs-2\"> \n                     <div align='right'><b>Report Ref.No</b></div>\n                     </div>\n                     <div class=\"col-xs-4\">\n                    <div align='left'> :{{doc.name or ''}}</div>\n                     </div>\n                </div>\n        <div class=\"col-xs-12\">\n\n            <table class=\"table table-bordered\" border=\"1\">\n                <tr>\n                    <td rowspan=\"2\" colspan=\"1\"><b>S. No.</b></td>\n                    <td rowspan=\"2\" colspan=\"1\"><b>Core ID</b></td>\n                    <td rowspan=\"2\" colspan=\"1\"><b>Core Description</b></td>\n                    <td colspan=\"1\"><b>Core Dia</b></td>\n                    <td colspan=\"1\"><b>Core Caped</b></td>\n                    <td rowspan=\"2\" colspan=\"1\"><b>L/D Ratio</b></td>\n                    <td colspan=\"1\"><b>Failure</b></td>\n                    <td colspan=\"1\"><b>Correction</b></td>\n                    <td colspan=\"2\"><b>Corrected Strength </b></td>\n                    <td colspan=\"2\"><b>Equivalent Cube Strength </b></td>\n                </tr>\n                <tr>\n                    <td colspan=\"1\"><b>(cm)</b></td>\n                    <td colspan=\"1\"><b>Length (cm)</b></td>\n                    <td colspan=\"1\"><b>Load, kN</b></td>\n                    <td colspan=\"1\"><b>Factor</b></td>\n                    <td colspan=\"1\"><b>N/mm2</b></td>\n                    <td colspan=\"1\"><b>kg/cm2</b></td>\n                    <td colspan=\"1\"><b>N/mm2</b></td>\n                    <td colspan=\"1\"><b>kg/cm2</b></td>\n                </tr>\n                <tr>\n                    <td colspan=\"1\">1</td>\n                    <td colspan=\"1\">{{doc.core_no}}</td>\n                    <td colspan=\"1\">{{doc.description}}</td>\n                    <td colspan=\"1\">{{doc.core_diameter}}</td>\n                    <td colspan=\"1\">{{doc.capped_len}}</td>\n                    <td colspan=\"1\">{{doc.lcd}}</td>\n                    <td colspan=\"1\">{{doc.failure_load}}</td>\n                    <td colspan=\"1\">{{doc.strength_corr}}</td>\n                    <td colspan=\"1\">{{doc.corr_strength}}</td>\n                    <td colspan=\"1\">{{doc.corr_strength_kg}}</td>\n\n                    <td colspan=\"1\">{{doc.eqv_cube_strength1}}</td>\n                    <td colspan=\"1\">{{doc.eqv_cube_strength2}}</td>\n\n\n\n                </tr>\n\n\n            </table>\n\n\n\n\n        </div>\n  \n        <div class=\"col-xs-12\">\n            <p><b></b></p>\n            {% set address= doc.note %}\n            {% if address %}\n            {% set list1 = address.split(\"\\n\") %}\n            {% endif %}\n            {% for list in list1 %}\n            <p>{{list}}</p>\n            {% endfor %}\n\n        </div>\n    </div>\n    <br><br><br><br><br><footer id=\"footer-html\">\n\n    <div class=\"row\">\n        <div class=\"col-xs-8\"><b>CALIBRATED BY</b><br><br><br><BR> Lab. Technician</div>\n\n        <div class=\"col-xs-4\"><b>CHECKED BY</b><br><br><br><BR>Laboratory Manager </div>\n    </div>\n</footer>\n</div>",
