n                   <div align='right'>  <b>Report Ref.No</b> </div>\n                     </div>\n                     <div class=\"col-xs-3\">\n                     :{{doc.name or ''}}<br>\n                     </div>\n                  \n                  </div>\n            <div class=\"row\"> \n               <div class=\"col-xs-2\"> \n                  <div align='right'> <b>Sample Desc</b> </div>\n                     </div>\n                     <div class=\"col-xs-4\">\n                   <div align='left'>  :{{doc.sample_desc}} </div>\n                     </div>\n                  </div>\n           <div class=\"col-xs-12\"> \n           <table class=\"table table-bordered\" border=\"1\">\n               <tr>\n                   <th colspan=\"2\"><b><center>Item</center></b></th>\n                   <th colspan=\"2\"><b><center>Description</center>\t</b></th>\n                   <th colspan=\"2\"><b><center>1</center>\t</b></th>\n                   <th colspan=\"2\"><b><center>2</center></b></th>\n                     <th colspan=\"2\"><b><center>3</center>\t</b></th>\n                    <th colspan=\"2\"><b><center>Average</center>\t</b></th>\n              </tr>\n                         \n               \n               \n                 <tr>\n                    <td colspan=\"2\"><center>A</center></td>\n                     <td colspan=\"2\"><center>Weight of Oven Dry Sample (g)</center>\t</td>\n                 \n                 {%- for row in doc.specific_gravity -%}\n                    <td colspan=\"2\"><center>{{row.weight_of_oven}}</center></td>\n                    {%- endfor -%}\n                    <td colspan=\"2\"><center></center>\t</td>\n                    <td colspan=\"2\"><center></center>\t</td>\n                    <td colspan=\"2\"><center></center>\t</td>\n                        <!--<td colspan=\"2\"><center>{{doc.average1}}</center>\t</td>-->\n                </tr>\n                \n                 <tr>\n                   