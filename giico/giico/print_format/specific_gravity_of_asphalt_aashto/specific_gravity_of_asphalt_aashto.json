 "html": "<div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <h4><center><b>REPORT ON SPECIFIC GRAVITY OF ASPHALT<br> AASHTO T 209 / ASTM D 2041\t</b></center></h4>\n        </div>\n        <div class=\"col-xs-7\">\n            <b>Project  :</b>{{doc.project}}<br>\n             <b>Client  :</b>{{doc.client}}<br>\n              <b>Sample Desc:  </b>{{doc.sample_desc}}<br>\n               <b>Location  :</b>{{doc.location}}<br>\n            </div>\n              <div class=\"col-xs-5\">\n            <b>Test Date :</b>{{frappe.utils.formatdate(doc.test_date)}}<br>\n             <b>Report Date :</b>{{frappe.utils.formatdate(doc.report_date)}}<br>\n              <b>Report Ref No:</b>{{doc.report_ref}}<br>\n               \n            </div>\n           <div class=\"col-xs-12\"> \n           <table class=\"table table-bordered\" border=\"1\">\n               <tr>\n                   <th colspan=\"2\"><b><center>Item</center></b></th>\n                   <th colspan=\"2\"><b><center>Description</center>\t</b></th>\n                   <th colspan=\"2\"><b><center>1</center>\t</b></th>\n                   <th colspan=\"2\"><b><center>2</center></b></th>\n                     <th colspan=\"2\"><b><center>3</center>\t</b></th>\n                    <th colspan=\"2\"><b><center>Average</center>\t</b></th>\n              </tr>\n                         \n               \n               \n                 <tr>\n                    <td colspan=\"2\"><center>A</center></td>\n                     <td colspan=\"2\"><center>Weight of Oven Dry Sample (g)</center>\t</td>\n                 \n                 {%- for row in doc.specific_gravity -%}\n                    <td colspan=\"2\"><center>{{row.weight_of_oven}}</center></td>\n                    {%- endfor -%}\n                        <td colspan=\"2\"><center>{{doc.average1}}</center>\t</td>\n                </tr>\n                \n                 <tr>\n                    <td colspan=\"2\"><center>B</center></td>\n                 <td colspan=\"2\"><center>Weight of Bottle + Water + Sample (g)\t</center></td>\n                 {%- for row in doc.specific_gravity -%}\n                    <td colspan=\"2\"><center>{{row.weight_of_sample}}</center></td>\n                    {%- endfor -%}\n                       <td colspan=\"2\"><center>{{doc.average2}}</center>\t</td>\n                </tr>\n                 <tr>\n                    <td colspan=\"2\"><center>C</center></td>\n                     <td colspan=\"2\"><center>Weight of Bottle + Water  (g)</center>\t</td>\n                     \n                    {%- for row in doc.specific_gravity -%}\n                    <td colspan=\"2\"><center>{{row.weight_of_bottle}}</center></td>\n                    {%- endfor -%}\n                     <td colspan=\"2\"><center>{{doc.average3}}</center>\t</td>\n                </tr>\n                 <tr>\n                    <td colspan=\"2\"><center>D</center></td>\n                      <td colspan=\"2\"><center>Apparent Specific Gravity</center>\t</td>\n                    {%- for row in doc.specific_gravity -%}\n                    <td colspan=\"2\"><center>{{row.apparent}}</center></td>\n                    {%- endfor -%}\n                    <td colspan=\"2\"><center>{{doc.average4}}</center>\t</td>\n                </tr>\n                         \n                     \n                     \n           </table><br>\n            \n    </div>\n    </div>\n     <div align=\"left\"><b>Remarks</b></div>\n             <div class=\"col-xs-12\">{{doc.remarks}}</div>\n           \n           <br><br><br><br>\n           <div class=\"row\">\n    <div class=\"col-xs-8\">Tested by :<br><br><br>Laboratory Technician\t\t\t</div>\n    \n