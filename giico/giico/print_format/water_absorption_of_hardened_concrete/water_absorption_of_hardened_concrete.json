 "html": "<meta name=\"pdfkit-orientation\" content=\"landscape\"/>\n<div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n     <div class=\"row\">\n    <div class=\"col-xs-12\">\n       <h3><b><center>WATER ABSORPTION OF HARDENED CONCRETE (BS 1881 : PART 122 : 1983)\t</center></b></h3><br></div>\n      <div class=\"col-xs-6\"><b>Contractor :</b>{{doc.contractor or \"\"}}<br>\n      <b>Consultant :</b>{{doc.consultant or \"\"}}<br>\n      <b>Owner :</b>{{doc.owner or \"\"}}<br>\n      <b>Project :</b>{{doc.project or \"\"}}<br>\n      <b>Location :</b>{{doc.location or \"\"}}<br>\n      <b>Report Ref. :</b>{{doc.report_ref or \"\"}}<br>\n      </div>\n      <div class=\"col-xs-6\"><b>Date of Cast :</b>{{frappe.utils.formatdate(doc.date_of_cast)or \"\"}}<br>\n      <b>Test Date :</b>{{frappe.utils.formatdate(doc.test_date)or \"\"}}<br>\n      <b>Report Date :</b>{{frappe.utils.formatdate(doc.report_date)or \"\"}}<br>\n       <b>Concrete Grade :</b>{{doc.concrete_grade or \"\"}}<br>\n      <b>\tConcrete Supplier :</b>{{doc.concrete_supplier or \"\"}}<br><br></div>\n         <div class=\"col-xs-12\">\n        <table class=\"table table-bordered\" border=\"1\">\n            <tr>\n                <th colspan=\"2\"><b><center>Sample ID</center></b></th>\n                  <th colspan=\"2\"><b><center>Diameter(mm)</center></b></th>\n                    <th colspan=\"2\"><b><center>Height(mm)</center></b></th>\n                      <th colspan=\"1\"><b><center>Dry Wt(g) *</center></b></th>\n                        <th colspan=\"1\"><b><center>Surface Dry Wt+ (g)</center></b></th>\n                          <th colspan=\"1\"><b><center>Submerged Wt (g)(water)</center></b></th>\n                            <th colspan=\"1\"><b><center>Density (kg/m3)</center></b></th>\n        