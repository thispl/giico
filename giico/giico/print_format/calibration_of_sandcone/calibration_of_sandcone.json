 "html": "<div class=\"container\">\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <h2><b><center>REPORT ON CALIBRATION OF SANDCONE (ASTM D 1556)\t</center></b></h2><br><br>\n        </div>\n        <div class=\"col-xs-8\"><b>CLIENT NAME :</b>{{doc.client}}<br><b>PROJECT NAME :</b>{{doc.project}}<br><b>EQUIPMENT ID NO :</b>{{doc.equip_id}}<br><br><br></div>\n          <div class=\"col-xs-4\"><b>DATE OF CALIB. :</b>{{frappe.utils.formatdate(doc.date_calib)}}<br><b>DATE OF REPORT :</b>{{frappe.utils.formatdate(doc.date_report)}}<br><b>GIICO REPORT NO :</b>{{doc.giico_report}}<br><br><br></div>\n         </div>\n         <div class=\"row\">\n             <div class=\"col-xs-12\">\n         <h6><b><u>MEASURING CYLINDER</u></b></h6>\n         </div>\n         <div class=\"col-xs-8\"><b>Weight (g) :</b>{{doc.weight}}<br><b>Volume (cm3) :</b>{{doc.volume}}<br><br><br></div>\n    </div>\n    <div class=\"col-xs-12\">\n        <h5><b>Determination of Density of Sand</b></h5>\n        <table class=\"table table-bordered\" border=\"1\">\n            <tr>\n                <td colspan=\"3\"><b>Trial No.</b></td>\n                 <td colspan=\"3\"><b>Wt of Sand + Cylinder (g)</b></td>\n                  <td colspan=\"3\"><b>Wt of Sand in Cylinder (g)</b></td> \n                  <td colspan=\"3\"><b>Bulk Density of Sand (g/cm3)</b></td>\n                  {%-for row in doc.determin_density-%}\n            </tr>\n            <tr>\n                <td colspan=\"3\">{{loop.index}}</td>\n                  <td colspan=\"3\">{{row.wt_cylinder}}</td>\n                    <td colspan=\"3\">{{row.wt_sand}}</td>\n                      <td colspan=\"3\">{{row.bulk_density}}</td>\n            </tr>\n            {%-endfor-%}\n            <tr>\n                <td colspan=\"9\"><b>Average</b></td>\n                 <td colspan=\"3\"><b>{{doc.aver}}</b></td>\n            </tr>\n        </table>\n    </div>\n     <div class=\"col-xs-12\">\n        <h5><b>Determination Of Weight Of Sand In Cone and Plate</b></h5>\n        <table class=\"table table-bordered\" border=\"1\">\n            <tr>\n                <td colspan=\"3\"><b>Trial No.</b></td>\n                 <td colspan=\"3\"><b>Wt of Sand + Bottle Before Pouring (g)</b></td>\n                  <td colspan=\"3\"><b>Wt of Sand + Bottle After Pouring (g)</b></td> \n                  <td colspan=\"3\"><b>Wt of Sand in Cone & Plate (g)</b></td>\n                  {%-for row in doc.determin_cone_plate-%}\n            </tr>\n            <tr>\n                <td colspan=\"3\">{{loop.index}}</td>\n                  <td colspan=\"3\">{{row.wt_bottle_before}}</td>\n                    <td colspan=\"3\">{{row.wt_bottle_after}}</td>\n                      <td colspan=\"3\">{{row.wt_cone_plate}}</td>\n            </tr>\n            {%-endfor-%}\n             <tr>\n                <td colspan=\"9\"><b>Average</b></td>\n                 <td colspan=\"3\"><b>{{doc.aver1}}</b></td>\n            </tr>\n        </table>\n    </div>\n    <div align=\"left\"><b>Remarks</b></div>\n             <div class=\"col-xs-12\">{{doc.remarks}}</div>\n           \n           <br><br><br><br>\n           \n           <div class=\"row\">\n    <div class=\"col-xs-8\"><b>CALIBRATED BY</b><br><br><br>Sr. Lab Technican\t\t</div>\n    \n     <div class=\"col-xs-4\"><b>CHECKED BY</b><br><br><br>Laboratory Manager\t\n</div>\n    \n</div>\n</div>",
