 "html": "<div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <h3><center><b>SCHMIDT HAMMER TEST</b></center></h3>\n            <h5><center><b>REBOUND NUMBER - R (ASTM-C-805)\t</b></center><br></h5>\n            \n        </div>\n        <div class=\"col-xs-7\">\n            <b>Project Name : </b>{{doc.project or \"\"}}<br>\n            <b>Location : </b>{{doc.location or \"\"}}<br>\n               <b>Structure Location : </b>{{doc.structure_location or \"\"}}<br>\n              \n        </div>\n        <div class=\"col-xs-5\">\n            <b>DATE OF TEST :</b>{{frappe.utils.formatdate(doc.date_of_test)or \"\"}}<br>\n             <b>DATE OF REPORT :</b>{{frappe.utils.formatdate(doc.date_of_report)or \"\"}}<br>\n              <b>JOB REF. \u2116 :</b>{{doc.job_ref or \"\"}}<br>\n               \n        </div>\n         <div class=\"col-xs-12\"> \n         <b>Consultant Name : </b>{{doc.consultant_name or \"\"}}<br>\n             </div>\n              <div class=\"col-xs-12\"> \n              <table class=\"table table-bordered\" border=\"1\">\n                  <tr>\n                      <th colspan=\"4\"><b>TEST NO:</b></th>\n                      <th colspan=\"2\">{{doc.test_no or \"\"}}</th>\n                       <th colspan=\"2\">{{doc.testno_2 or \"\"}}</th>\n                        <th colspan=\"2\">{{doc.testno_3 or \"\"}}</th>\n                         <th colspan=\"2\">{{doc.testno_4 or \"\"}}</th>\n                  </tr>\n                  <tr>\n                      <td colspan=\"4\"><b>LOCATION:</b></td>\n                      <td colspan=\"2\">{{doc.location_1 or \"\"}}</td>\n                      <td colspan=\"2\">{{doc.location_2 or \"\"}}</td>\n                      <td colspan=\"2\">{{doc.location_3 or \"\"}}</td>\n                      <td colspan=\"2\">{{doc.location_4 or \"\"}}</td>\n                  </tr>\n                   <tr>\n                      <td colspan=\"4\"><b>DIRECTION OF TEST</b></td>\n                      <td colspan=\"2\"></td>\n                      <td colspan=\"2\"></td>\n                      <td colspan=\"2\"></td>\n                      <td colspan=\"2\"></td>\n                  </tr>\n                   <tr>\n                      <td colspan=\"12\"><b>READINGS</b></td>\n                  </tr>\n                    {%- for row in doc.readings_4 -%}\n                    \n                         \n                     <tr>\n                      <td colspan=\"4\">{{loop.index}}</td>\n                      <td colspan=\"2\">{{row.test1 or \"\"}}</td>\n                       <td colspan=\"2\">{{row.test2 or \"\"}}</td>\n                        <td colspan=\"2\">{{row.test3 or \"\"}}</td>\n                         <td colspan=\"2\">{{row.test4 or \"\"}}</td>\n                      </tr>\n                      {%- endfor -%}\n                      \n                        <tr>\n                      <td colspan=\"12\"><b>ADDITIONAL READINGS</b></td>    \n                      </tr>\n                       {%- for row in doc.additional_readings4 -%}\n                      \n                      <tr>\n                          <td colspan=\"4\">{{loop.index}}</td>\n                            <td colspan=\"2\">{{row.add_test1 or \"\"}}</td> \n                               <td colspan=\"2\">{{row.add_test2 or \"\"}}</td> \n                                <td colspan=\"2\">{{row.add_test3 or \"\"}}</td>\n                                  <td colspan=\"2\">{{row.add_test4 or \"\"}}</td>\n                                    {%- endfor -%}\n                      </tr>\n               \n                 <tr>\n                     <td colspan=\"4\"><b>AVE.(10Readgs)</b></td>\n                       <td colspan=\"2\"