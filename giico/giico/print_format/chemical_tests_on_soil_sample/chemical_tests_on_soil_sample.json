 "html": "<div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n     <div class=\"row\">\n    <div class=\"col-xs-12\">\n        <h4><center><b>REPORT OF CHEMICAL TESTS ON SOIL SAMPLES\t</b></center></h4>\n     </div>\n     \n      <div class=\"row\">\n        <div class=\"col-xs-2\">\n           <div align='right'> <b>Client</b></div>\n            </div>\n            <div class=\"col-xs-4\">\n            <div align='left'>:{{doc.customer}}</div>\n            </div>\n               <div class=\"col-xs-3\">\n                <div align='right'>    <b>IR No</b></div>\n                </div>\n                <div class=\"col-xs-3\">\n                    :{{doc.ir_no or \"\"}}<br>\n                </div>\n                </div>\n              <div class=\"row\"> \n               <div class=\"col-xs-2\">\n           <div align='right'>  <b>Project Name</b></div>\n            </div>\n               <div class=\"col-xs-4\">\n                <div align='left'>    :{{doc.project_client or ''}}</div>\n                   </div>\n                    <div class=\"col-xs-3\">\n                   <div align='right'> <b>Test Date</b></div>\n                   </div>\n                    <div class=\"col-xs-3\">\n                        :{{doc.get_formatted(\"date_test\")}}<br>\n                   </div>\n                  </div> \n                  \n                 <div class=\"row\"> \n                  <div class=\"col-xs-2\"> \n                  <div align='right'>  <b>Description</b></div>\n                     </div>\n                     <div class=\"col-xs-4\">\n                    <div align='left'>  :{{doc.description}}</div>\n                     </div>\n               <div class=\"col-xs-3\">   \n                   <div align='right'>  <b>Report Date</b></div>\n           