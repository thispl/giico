 "html": "<div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <h4><b><center>REPORT OF CHEMICAL TESTS ON SOIL SAMPLE\t\t</center></b></h4>\n            </div>\n            \n             <div class=\"row\">\n        <div class=\"col-xs-2\">\n           <div align='right'> <b>Client</b></div>\n            </div>\n            <div class=\"col-xs-4\">\n            <div align='left'>:{{doc.customer}}</div>\n            </div>\n                <div class=\"col-xs-3\">\n                  <div align='right'> <b>Test Date</b></div>\n                   </div>\n                    <div class=\"col-xs-3\">\n                        :{{frappe.utils.formatdate(doc.testing_date)}}<br>\n                   </div>\n               \n                </div>\n              <div class=\"row\"> \n               <div class=\"col-xs-2\">\n           <div align='right'> <b>Project Name</b></div>\n            </div>\n               <div class=\"col-xs-4\">\n                 <div align='left'>  :{{doc.project_name or ''}}</div>\n                   </div>\n                <div class=\"col-xs-3\">   \n                  <div align='right'>  <b>Report Date</b></div>\n                    </div>\n                    <div class=\"col-xs-3\">   \n                    :{{frappe.utils.formatdate(doc.reporting_date) or ''}}<br>\n                  </div>\n                  </div> \n                  \n                 <div class=\"row\"> \n                   <div class=\"col-xs-2\"> \n                  <div align='right'> <b>Owner</b></div>\n                     </div>\n                     <div class=\"col-xs-4\">\n                    <div align='left'> :{{doc.owner or \"\"}}<br></div>\n                     </div>\n                     <div class=\"col-xs-3\">\n                <div align='right'>    <b>Location </b></div>\n                </div>\n                <div class=\"col-xs-3\">\n                    :{{doc.location}}<br>\n                </div>\n                   \n                  </div>\n            <div class=\"row\"> \n               <div class=\"col-xs-2\"> \n                 <div align='right'>  <b> Description</b></div>\n                     </div>\n                     <div class=\"col-xs-4\">\n                  <div align='left'>   :{{doc.description}}</div>\n                     </div>\n                     <div class=\"col-xs-3\"> \n                   <div align='right'>  <b>Report Ref No</b></div>\n                     </div>\n                     <div class=\"col-xs-3\">\n                     :{{doc.name or \"\"}}<br>\n                     </div>\n                     </div>\n            <div class=\"col-xs-12\">\n                    <table class= \"table table-bordered\" border=\"1\">\n                    <tr>\n                        <th colspan=\"4\"><b><center>Test Parameter\t</center>\t</b></th>\n                        <th colspan=\"4\"><b><center>Result %\t</center></b></th>\n                        <th colspan=\"4\"><b><center>Ref Standard</center></b></th>\n                    </tr>\n                    <tr>\n                        <td colspan=\"4\"><center>{{doc.test_parameter or \"\"}}</center></td>\n                           <td colspan=\"4\"><center>{{doc.result or \"\"}}</center></td>\n                              <td colspan=\"4\"><center>{{doc.ref_standard or \"\"}}</center></td>\n                    </tr>\n                    </table>\n            \n</div>\n    </div>\n     <div align=\"left\"><b>Remarks</b></div>\n             <div class=\"col-xs-12\">\n                 {% set address = doc.remarks or \"\" %}\n                 {% if address %}\n                 {% set list1= address.split(\"\\n\") %}\n                 {% endif %}\n                 {% for