 "html": "<meta name=\"pdfkit-orientation\" content=\"Landscape\" />\n<div class=\"container\">\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n            {% if letter_head %}\n            {{ letter_head.content }}\n\n            {% endif %}\n        </div>\n    </div>\n\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <h4>\n                <center>SUMMARY OF POINT LOAD TEST RESULTS</center>\n            </h4>\n        </div>\n    </div>\n    <div class=\"row\">\n        <div class=\"col-xs-18\">\n            <table class=\"table table-bordered\" border=\"1\">\n                <tr>\n                    <th rowspan=\"2\" colspan=\"2\"><center>S.No</center></th>\n                    <th rowspan=\"2\" colspan=\"2\"><center>Sample ID</center></th>\n                    <th rowspan=\"2\" colspan=\"2\"><center>Date</center></th>\n                    <th colspan=\"2\"><center>Point Load Index</center></th>\n                    <th colspan=\"2\"><center>Average</center></th>\n                    <th colspan=\"1\"><center>*Uniaxial</center></th>\n                    <th colspan=\"1\"><center>Average</center></th>\n\n                </tr>\n                <tr>\n                    <th colspan=\"2\"><center>I50 : Mpa</center></th>\n                    <th colspan=\"2\"><center>Mpa</center></th>\n                    <th colspan=\"1\"><center>Comp str. Mpa</center></th>\n                    <th colspan=\"1\"><center>Mpa</center>\n                    </th>\n                </tr>\n\n                {% set plt = frappe.get_all(\"Point Load Test\",filters ={\"project\":\"SAL-ORD-2020-00003\"}) %}\n                {% for p in plt %}\n                {% set pl = frappe.get_doc(\"Point Load Test\",p) %}\n\n                {% if pl.project == \"SAL-ORD-2020-00003\" %}\n                   {% set total = [] %}\n             {% set avg =[0] %}\n             {% set value = [] %}\n                    \n                        {% if total.append((pl.point_load_value)) %}\n                \n                    {% endif %}\n                  \n     \n                <tr>\n                    <td colspan=\"2\"><center>{{loop.index or \"\"}}</center></td>\n                    <td colspan=\"2\"><center>{{pl.bh_no or \"\"}}</center></td>\n                    <td colspan=\"2\"><center>{{frappe.utils.formatdate(pl.date) or \"\"}}</center></td>\n                    <td colspan=\"2\"><center>{{ pl.point_load_value or \"\"}}</center></td>\n                    <td colspan=\"2\"><center></center></td>\n                    <td colspan=\"1\"><center>{{pl.uni_cs1 or \"\"}}</center></td>\n                         <!--{% for t in total %}-->\n                    <td colspan=\"1\"><center>{{total}}</center>\n                       <!--{% endfor %}-->\n</td>\n                    \n                    {% endif %}\n                    {% endfor %}\n                </tr>\n\n                <tr>\n                    <td colspan=\"2\"></td>\n                    <td colspan=\"2\"></td>\n                    <td colspan=\"2\"></td>\n                    <td colspan=\"2\"></td>\n                    <td colspan=\"2\"></td>\n                    <td colspan=\"1\"></td>\n                    <td colspan=\"1\"></td>\n                </tr>\n\n            </table>\n            <h4>Remarks:</h4><br>\n            <p>*Uniaxial comp strength = 22 times Point load Index strength<br>\n                (Brook,1985)<br>\n                CIRIA Spl Publcn-83</p><br>\n        </div><br>\n    </div>\n    <div class=\"row\">\n        <div class=\"col-xs-6\"><b>Project:</b>{{doc.project or \"\"}}<br>\n            <b> Client:</b>{{doc.customer or \"\"}}<br>\n        </div><br><br>\n</div>\n  <div class=\"row\">\n        <div class=\"col-xs-8\"><b>Tested By:</b><br></div>\n           <div class=\"col-xs-4\">   <b>Checked By:</b></div>\n    </div>\n</div>",
