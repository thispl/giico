 "html": "<meta name=\"pdfkit-orientation\" content=\"Landscape\"/>\n<div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n        <h3><b><center>\tREPORT ON CALIBRATION OF BALANCE & WEIGHT (ASTM - D 4753)\t</center></b></h3>\n<br>\n        </div>\n          <div class=\"row\">\n        <div class=\"col-xs-2\">\n            <div align='right'>  <b>CLIENT NAME </b></div>\n            </div>\n            <div class=\"col-xs-4\">\n             <div align='left'> :{{doc.customer or ''}}</div>\n            </div>\n               <div class=\"col-xs-3\">\n                 <div align='right '><b>READABILITY</b></div>\n                </div>\n                <div class=\"col-xs-3\">\n                    :{{doc.read or ''}}<br>\n                </div>\n                </div>\n              <div class=\"row\"> \n               <div class=\"col-xs-2\">\n            <div align='right '><b>PROJECT NAME</b></div>\n            </div>\n               <div class=\"col-xs-4\">\n                 <div align='left '>  :{{doc.project_name or ''}}</div>\n                   </div>\n                    <div class=\"col-xs-3\">\n                   <div align='right '><b>CALIBRATION DATE</b></div>\n                   </div>\n                    <div class=\"col-xs-3\">\n                        :{{frappe.utils.formatdate(doc.calibration_date) or ''}}<br>\n                   </div>\n                  </div> \n                 <div class=\"row\"> \n               <div class=\"col-xs-2\">   \n                 <div align='right '>  <b>MAKE/BRAND  </b></div>\n                   </div>\n                   <div class=\"col-xs-4\">   \n                  <div align='left'> :{{doc.make_brand or ''}}</div>\n                   </div>\n                   <div class=\"col-xs-3\">   \n                   <div align='right '> <b>REPORT DATE</b></div>\n                    </div>\n                    <div class=\"col-xs-3\">   \n                    :{{frappe.utils.formatdate(doc.report_date) or ''}}<br>\n                  </div>\n                  </div>\n            <div class=\"row\"> \n               <div class=\"col-xs-2\">     \n                   <div align='right '> <b>CAPACITY </b></div>\n                    </div>\n                     <div class=\"col-xs-4\">     \n                   <div align='left '> :{{doc.capacity or ''}}</div>\n                    </div>\n                     <div class=\"col-xs-3\"> \n                    <div align='right '> <b>REPORT REF.NO</b></div>\n                     </div>\n                     <div class=\"col-xs-3\">\n                     :{{doc.name or ''}}<br>\n                     </div>\n                </div>\n            <div class=\"row\"> \n               <div class=\"col-xs-2\">\n            <div align='right '><b>EQUIPMENT ID/S.N.</b></div>\n            </div>\n             <div class=\"col-xs-4\">\n            <div align='left'>:{{doc.equip_id or ''}}</div>\n            </div>\n        </div>\n        </div>\n        </div>\n        <div class=\"col-xs-12\">\n            <table class=\"table table-bordered\" border=\"1\">\n                <tr>\n                    <th  rowspan=\"2\" colspan=\"2\"><b>SR.No</b></th>\n                    <th  rowspan=\"2\" colspan=\"2\"><b>STANDARD WEIGHT (g)</b></th>\n                    <th rowspan=\"2\" colspan=\"2\"><b>BALANCE WEIGHT(g)</b></th>\n                    <th  colspan=\"4\"><b>DIFFERENCE\t</b></th>\n                    <th  rowspan=\"2\" colspan=\"2\"><b>REMARKS</b></th>\n                </tr>\n                <tr>\n                    <th colspan=\"2\"><b>(g)</b></th>\n                     <th colspan=\"2\"><b>(%)</b></th>\n                     {%- for row in doc.electronic_balance -%}\n                </tr>\n                <tr>\n                    <td colspan=\"2\">{{loop.index or ''}}</td>\n                       <td colspan=\"2\">{{row.standard_weight or ''}}</td>\n                          <td colspan=\"2\">{{row.balance_weight or ''}}</td>\n                             <td colspan=\"2\">{{row.differenceg or ''}}</td>\n                                <td colspan=\"2\">{{row.difference or ''}}</td>\n                                   <td colspan=\"2\">{{row.remarks or ''}}</td>\n                </tr>\n                {%- endfor -%}\n            </table>\n        </div>\n        \n        </div>\n         <div align=\"left\"><b>Remarks</b></div>\n             <div class=\"col-xs-12\">\n              {% set address= doc.remarks %}\n              {% if address %}\n              {% set list1 = address.split(\"\\n\") %} \n              {% endif %}\n              {% for list in list1 %}\n              <p>{{list}}</p>\n                 {% endfor %}\n                 \n             </div>\n           \n           <br><br><br><br><br>\n           <div class=\"row\">\n    <div class=\"col-xs-8\"><b>CALIBRATED BY</b><br><br><br><BR>\tLab. Technician</div>\n    \n     <div class=\"col-xs-4\"><b>CHECKED BY</b><br><br><br><BR>Laboratory Manager\t</div>\n</div>\n           \n           \n        </div>",
