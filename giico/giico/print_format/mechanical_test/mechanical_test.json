 "html": "<meta name=\"pdfkit-orientation\" content=\"Landscape\"/>\n<div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n     <div class=\"row\">\n    <div class=\"col-xs-12\">\n        <h3><center><b>MECHANICAL TEST REPORTS (BS 4449 Gr 500B )\t</b></center></h3><br>\n     </div>\n     <div class=\"col-xs-7\">\n         <b>Main Contractor : </b>{{doc.main_con or \"\"}}<br>\n          <b>Project  : </b>{{doc.project or \"\"}}<br>\n           <b>Description : </b>{{doc.description or \"\"}}<br>\n            <b>Material : </b>{{doc.material or \"\"}}<br><br>\n            \n      </div>\n      <div class=\"col-xs-5\">\n           <b>Sample in Lab Date :</b>{{frappe.utils.formatdate(doc.sample_lab)or \"\"}}<br>\n            <b>Date of Test :</b>{{frappe.utils.formatdate(doc.date_test)or \"\"}}<br>\n             <b>Giico. RFI. No. :</b>{{doc.giico or \"\"}}<br>\n              <b>Thick / Dia : </b>{{doc.thick or \"\"}}mm<br>\n              <b>Lab Temp.  : </b>{{doc.lab_temp or \"\"}}oC<br>\n      </div>\n       <div class=\"col-xs-12\">\n           <table class=\"table table-bordered\" bordr=\"1\">\n               <tr>\n                   <th colspan=\"12\"><b><center>TENSION TEST </center></b></th>\n               </tr>\n               <tr>\n                   <th colspan=\"2\"><b><center>SI.No.</center></b></th>\n                   <th colspan=\"5\"><b><center>Description</center>\t</b></th>\n                   <th colspan=\"5\"><b><center>Test Result</center>\t</b></th>\n               </tr>\n               {%- for row in doc.tension_test -%}\n               <tr>\n               <td colspan=\"2\"><center>{{loop.index}}</center></td>\n                 <td colspan=\"5\"><center>{{row.description or \"\"}}</center></td>\n                   <td colspan=\"5\"><center>{{row.test_result or \"\"}}</center></td>\n                   {%- endfor -%}\n               <tr>\n                   <tr>\n                       <th colspan=\"12\"><b><center>Requirements </center></b></th>\n                   </tr>\n                   <tr>\n                       <th  rowspan=\"2\" colspan=\"3\"><b><center>Material Spec.\t</center></b></th>\n                       <th colspan=\"2\"><b><center>Yield Strength</center></b>\t</th>\n                       <th colspan=\"2\"><center><b>Elongation % Min</b>\t</center>\t</th>\n                         <th colspan=\"2\"><center><b>Tensile / Yield Ratio</b></center>\t</th>\n                          <th  rowspan=\"2\" colspan=\"3\"><center><b>Actual Ratio</b></center></th>\n                   </tr>\n                   <tr>\n                       <th colspan=\"1\"><b><center>Min (Mpa)</center></b></th>\n                         <th colspan=\"1\"><b><center>Max (Mpa)</center></b></th>\n                          <th colspan=\"1\"><b><center>Gauge Length</center>\t</b></th>\n                         <th colspan=\"1\"><b><center>F.Length</center></b></th>\n                          <th colspan=\"1\"><b><center>Min </center></b></th>\n                         <th colspan=\"1\"><center><b>Max</b></center></th>\n                         {%- for row in doc.requirements -%}\n                   </tr>\n                   <tr>\n                       <td colspan=\"3\"><center>{{row.material or \"\"}}</center></td>\n                        <td colspan=\"1\"><center>{{row.min_mpa or \"\"}}</center></td>\n                         <td colspan=\"1\"><center>{{row.max_mpa or \"\"}}</center></td>\n                          <td colspan=\"1\"><center>{{row.min_gauge or \"\"}}</center></td>\n                           <td colspan=\"1\"><center>{{row.min_f_length or \"\"}}</center></td>\n                            <td colspan=\"1\"><center>{{row.tensile or \"\"}}</center></td>\n                             <td colspan=\"1\"><center>{{row.ratio_max or \"\"}}</center></td>\n                              <td colspan=\"3\"><center>{{row.actual_ratio or \"\"}}</center></td>\n                          \n                   </tr>\n                   {%- endfor -%}\n                   <tr>\n                       <th colspan=\"12\"><center><b>BEND TEST </b></center></th>\n                   </tr>\n                   <tr>\n                       <th colspan=\"2\"><b><center>Spec. ID</center></b></th>\n                        <th colspan=\"2\"><b><center>Type</center></b></th>\n                         <th colspan=\"2\"><b><center>Bend Angle </center></b></th>\n                          <th colspan=\"2\"><b><center>Re-Bend Angle </center>\t</b></th>\n                           <th colspan=\"2\"><b><center>Plunger Dia </center>\t</b></th>\n                            <th colspan=\"2\"><b><center>Remarks</center> \t</b></th>\n                            {%- for row in doc.bend_test -%}\n                   </tr>\n                   \n                   <tr>\n                       <td colspan=\"2\"><center>{{row.spec_id or \"\"}}</center></td>\n                        <td colspan=\"2\"><center>{{row.type or \"\"}}</center></td>\n                         <td colspan=\"2\"><center>{{row.bend_angle or \"\"}}</center></td>\n                          <td colspan=\"2\"><center>{{row.re_bend_angle or \"\"}}</center></td>\n                           <td colspan=\"2\"><center>{{row.plunger_dia or \"\"}}</center></td>\n                            <td colspan=\"2\"><center>{{row.remark or \"\"}}</center></td>\n                   </tr>\n                   {%- endfor -%}\n           </table>\n           </div>\n      </div>\n      <div align=\"left\"><b>Remarks</b></div>\n             <div class=\"col-xs-12\">{{doc.remarks or \"\"}}</div>\n           \n           <br><br><br><br>\n           <div class=\"row\">\n    <div class=\"col-xs-8\"><b>Viewed by</b><br><br><br><br><b>QA/QC Engineer\t\t</b>\t</div>\n    \n     <div class=\"col-xs-4\"><b>Approved by</b><br><br><br><br><b>Laboratory Manager\t</b>\n</div>\n</div>\n      </div>",
