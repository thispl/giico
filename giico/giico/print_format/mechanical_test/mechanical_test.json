 "html": "<div class=\"container\">\n     <div class=\"row\">\n    <div class=\"col-xs-12\">\n        <h3><center><b>MECHANICAL TEST REPORTS (BS 4449 Gr 500B )\t</b></center></h3><br>\n     </div>\n     <div class=\"col-xs-7\">\n         <b>Main Contractor : </b>{{doc.main_con}}<br>\n          <b>Project  : </b>{{doc.project}}<br>\n           <b>Description : </b>{{doc.description}}<br>\n            <b>Material : </b>{{doc.material}}<br><br>\n            \n      </div>\n      <div class=\"col-xs-5\">\n           <b>Sample in Lab Date :</b>{{frappe.utils.formatdate(doc.sample_lab)}}<br>\n            <b>Date of Test :</b>{{frappe.utils.formatdate(doc.date_test)}}<br>\n             <b>Giico. RFI. No. :</b>{{doc.giico}}<br>\n              <b>Thick / Dia : </b>{{doc.thick}}mm<br>\n              <b>Lab Temp.  : </b>{{doc.lab_temp}}oC<br>\n      </div>\n       <div class=\"col-xs-12\">\n           <table class=\"table table-bordered\" bordr=\"1\">\n               <tr>\n                   <td colspan=\"12\"><b><center>TENSION TEST </center></b></td>\n               </tr>\n               <tr>\n                   <td colspan=\"2\"><b>SI.No.</b></td>\n                   <td colspan=\"5\"><b>Description\t</b></td>\n                   <td colspan=\"5\"><b>Test Result\t</b></td>\n               </tr>\n               {%- for row in doc.tension_test -%}\n               <tr>\n               <td colspan=\"2\">{{loop.index}}</td>\n                 <td colspan=\"5\">{{row.description}}</td>\n                   <td colspan=\"5\">{{row.test_result}}</td>\n                   {%- endfor -%}\n               <tr>\n                   <tr>\n                       <td colspan=\"12\"><b><center>Requirements </center></b></td>\n                   </tr>\n                   <tr>\n                       <td  rowspan=\"2\" colspan=\"3\"><b>Material Spec.\t</b></td>\n                       <td colspan=\"2\"><b>Yield Strength</b>\t</td>\n                       <td colspan=\"2\"><b>Elongation % Min</b>\t\t</td>\n                         <td colspan=\"2\"><b>Tensile / Yield Ratio</b>\t</td>\n                          <td  rowspan=\"2\" colspan=\"3\"><b>Actual Ratio</b></td>\n                   </tr>\n                   <tr>\n                       <td colspan=\"1\"><b>Min (Mpa)</b></td>\n                         <td colspan=\"1\"><b>Max (Mpa)</b></td>\n                          <td colspan=\"1\"><b>Gauge Length\t</b></td>\n                         <td colspan=\"1\"><b>F.Length</b></td>\n                          <td colspan=\"1\"><b>Min </b></td>\n                         <td colspan=\"1\"><b>Max</b></td>\n                         {%- for row in doc.requirements -%}\n                   </tr>\n                   <tr>\n                       <td colspan=\"3\">{{row.material}}</td>\n                        <td colspan=\"1\">{{row.min_mpa}}</td>\n                         <td colspan=\"1\">{{row.max_mpa}}</td>\n                          <td colspan=\"1\">{{row.min_gauge}}</td>\n                           <td colspan=\"1\">{{row.min_f_length}}</td>\n                            <td colspan=\"1\">{{row.tensile}}</td>\n                             <td colspan=\"1\">{{row.ratio_max}}</td>\n                              <td colspan=\"3\">{{row.actual_ratio}}</td>\n                          \n                   </tr>\n                   {%- endfor -%}\n                   <tr>\n                       <td colspan=\"12\"><center><b>BEND TEST </b></center></td>\n                   </tr>\n                   <tr>\n                       <td colspan=\"2\"><b>Spec. ID</b></td>\n                        <td colspan=\"2\"><b>Type</b></td>\n                         <td colspan=\"2\"><b>Bend Angle \t</b></td>\n                          <td colspan=\"2\"><b>Re-Bend Angle \t</b></td>\n                           <td colspan=\"2\"><b>Plunger Dia \t</b></td>\n                            <td colspan=\"2\"><b>Remarks \t</b></td>\n                            {%- for row in doc.bend_test -%}\n                   </tr>\n                   \n                   <tr>\n                       <td colspan=\"2\">{{row.spec_id}}</td>\n                        <td colspan=\"2\">{{row.type}}</td>\n                         <td colspan=\"2\">{{row.bend_angle}}</td>\n                          <td colspan=\"2\">{{row.re_bend_angle}}</td>\n                           <td colspan=\"2\">{{row.plunger_dia}}</td>\n                            <td colspan=\"2\">{{row.remark}}</td>\n                   </tr>\n                   {%- endfor -%}\n           </table>\n           </div>\n      </div>\n      <div align=\"left\"><b>Remarks</b></div>\n             <div class=\"col-xs-12\">{{doc.remarks}}</div>\n           \n           <br><br><br><br>\n           <div class=\"row\">\n    <div class=\"col-xs-8\">Viewed by<br><br><br>QA/QC Engineer\t\t\t</div>\n    \n     <div class=\"col-xs-4\">Approved by<br><br><br>Laboratory Manager\t\n</div>\n</div>\n      </div>",
