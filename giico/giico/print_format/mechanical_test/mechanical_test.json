 "html": "<meta name=\"pdfkit-orientation\" content=\"Landscape\"/>\n<div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n     <div class=\"row\">\n    <div class=\"col-xs-12\">\n        <h3><center><b>MECHANICAL TEST REPORTS (BS 4449 Gr 500B )\t</b></center></h3><br>\n     </div>\n     <div class=\"col-xs-7\">\n         <b>Main Contractor : </b>{{doc.main_con or \"\"}}<br>\n          <b>Project  : </b>{{doc.project or \"\"}}<br>\n           <b>Description : </b>{{doc.description or \"\"}}<br>\n            <b>Material : </b>{{doc.material or \"\"}}<br><br>\n            \n      </div>\n      <div class=\"col-xs-5\">\n           <b>Sample in Lab Date :</b>{{frappe.utils.formatdate(doc.sample_lab)or \"\"}}<br>\n            <b>Date of Test :</b>{{frappe.utils.formatdate(doc.date_test)or \"\"}}<br>\n             <b>Giico. RFI. No. :</b>{{doc.giico or \"\"}}<br>\n              <b>Thick / Dia : </b>{{doc.thick or \"\"}}mm<br>\n              <b>Lab Temp.  : </b>{{doc.lab_temp or \"\"}}oC<br>\n      </div>\n       <div class=\"col-xs-12\">\n           <table class=\"table table-bordered\" bordr=\"1\">\n               <tr>\n                   <th colspan=\"12\"><b><center>TENSION TEST </center></b></th>\n               </tr>\n               <tr>\n                   <th colspan=\"2\"><b>SI.No.</b></th>\n                   <th colspan=\"5\"><b>Description\t</b></th>\n                   <th colspan=\"5\"><b>Test Result\t</b></th>\n               </tr>\n               {%- for row in doc.tension_test -%}\n               <tr>\n               <td colspan=\"2\">{{loop.index}}</td>\n                 <td colspan=\"5\">{{row.description or \"\"}}</td>\n                   <td colspan=\"5\">{{row.test_result or \"\"}}</td>\n                   {%- endfor -%}\n               <tr>\n                   <tr>\n                       <th colspan=\"12\"><b><center>Requirements </center></b></th>\n                   </tr>\n                   <tr>\n                       <th  rowspan=\"2\" colspan=\"3\"><b>Material Spec.\t</b></th>\n                       <th colspan=\"2\"><b>Yield Strength</b>\t</th>\n                       <th colspan=\"2\"><b>Elongation % Min</b>\t\t</th>\n                         <th colspan=\"2\"><b>Tensile / Yield Ratio</b>\t</th>\n                          <th  rowspan=\"2\" colspan=\"3\"><b>Actual Ratio</b></th>\n                   </tr>\n                   <tr>\n                       <th colspan=\"1\"><b>Min (Mpa)</b></th>\n                         <th colspan=\"1\"><b>Max (Mpa)</b></th>\n                          <th colspan=\"1\"><b>Gauge Length\t</b></th>\n                         <th colspan=\"1\"><b>F.Length</b></th>\n                          <th colspan=\"1\"><b>Min </b></th>\n                         <th colspan=\"1\"><b>Max</b></th>\n                         {%- for row in doc.requirements -%}\n                   </tr>\n                   <tr>\n                       <td colspan=\"3\">{{row.material or \"\"}}</td>\n                        <td colspan=\"1\">{{row.min_mpa or \"\"}}</td>\n                         <td colspan=\"1\">{{row.max_mpa or \"\"}}</td>\n                          <td colspan=\"1\">{{row.min_gauge or \"\"}}</td>\n                           <td colspan=\"1\">{{row.min_f_length or \"\"}}</td>\n                            <td colspan=\"1\">{{row.tensile or \"\"}}</td>\n                             <td colspan=\"1\">{{row.ratio_max or \"\"}}</td>\n                              <td colspan=\"3\">{{row.actual_ratio or \"\"}}</td>\n                          \n                   </tr>\n                   {%- endfor -%}\n                   <tr>\n                       <th colspan=\"12\"><center><b>BEND TEST </b></center></th>\n                   </tr>\n                   <tr>\n                       <th colspan=\"2\"><b>Spec. ID</b></th>\n                        <th colspan=\"2\"><b>Type</b></th>\n                         <th colspan=\"2\"><b>Bend Angle \t</b></th>\n                          <th colspan=\"2\"><b>Re-Bend Angle \t</b></th>\n                           <th colspan=\"2\"><b>Plunger Dia \t</b></th>\n                            <th colspan=\"2\"><b>Remarks \t</b></th>\n                            {%- for row in doc.bend_test -%}\n                   </tr>\n                   \n                   <tr>\n                       <td colspan=\"2\">{{row.spec_id or \"\"}}</td>\n                        <td colspan=\"2\">{{row.type or \"\"}}</td>\n                         <td colspan=\"2\">{{row.bend_angle or \"\"}}</td>\n                          <td colspan=\"2\">{{row.re_bend_angle or \"\"}}</td>\n                           <td colspan=\"2\">{{row.plunger_dia or \"\"}}</td>\n                            <td colspan=\"2\">{{row.remark or \"\"}}</td>\n                   </tr>\n                   {%- endfor -%}\n           </table>\n           </div>\n      </div>\n      <div align=\"left\"><b>Remarks</b></div>\n             <div class=\"col-xs-12\">{{doc.remarks or \"\"}}</div>\n           \n           <br><br><br><br>\n           <div class=\"row\">\n    <div class=\"col-xs-8\">Viewed by<br><br><br>QA/QC Engineer\t\t\t</div>\n    \n     <div class=\"col-xs-4\">Approved by<br><br><br>Laboratory Manager\t\n</div>\n</div>\n      </div>",
