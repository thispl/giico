 "html": "<meta name=\"pdfkit-orientation\" content=\"landscape\">\n<div class=\"container\">\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n            {% if letter_head %}\n            {{ letter_head.content }}\n\n            {% endif %}\n        </div>\n    </div>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <h4><b>\n                    <center>REPORT ON CONCRETE CORE DRILLING (ASTM C 42) AND STRENGTH TESTING (ASTM C 39)</center>\n                </b><br><br></h4>\n            <div align=\"left\"><b>Project Details</b></div>\n\n        </div>\n        \n         <div class=\"row\">\n        <div class=\"col-xs-2\">\n           <div align='right'> <b>Consultant</b></div>\n            </div>\n            <div class=\"col-xs-4\">\n           <div align='left'> :{{doc.consultant_name}}</div>\n            </div>\n               <div class=\"col-xs-3\">\n                <div align='right'>    <b>Location </b></div>\n                </div>\n                <div class=\"col-xs-3\">\n                    :{{doc.location}}<br>\n                </div>\n                </div>\n              <div class=\"row\"> \n               <div class=\"col-xs-2\">\n            <div align='right'><b>Project Name</b></div>\n            </div>\n               <div class=\"col-xs-4\">\n                 <div align='left'>  :{{doc.project_name or ''}}</div>\n                   </div>\n                    <div class=\"col-xs-3\">\n                 <div align='right'>  <b>Test Date</b></div>\n                   </div>\n                    <div class=\"col-xs-3\">\n                        :{{doc.get_formatted(\"test_date\") or ''}}<br>\n                   </div>\n                  </div> \n                  \n                 <div class=\"row\"> \n               <div class=\"col-xs-2\"> \n                    <div align='right'> <b>Report Ref No</b></div>\n                     </div>\n                     <div class=\"col-xs-4\">\n                    <div align='left'> :{{doc.name or ''}}</div>\n                     </div>\n                   <div class=\"col-xs-3\">   \n                    <div align='right'><b>Report Date</b></div>\n                    </div>\n                    <div class=\"col-xs-3\">   \n                    :{{doc.get_formatted(\"report_date\") or ''}}<br>\n                  </div>\n                  </div>\n        <br>\n        <div class=\"col-xs-12\">\n            <div align=\"left\"><b>Core Details</b></div>\n        </div>\n        <br>\n        <br>\n        <div class=\"col-xs-7\">\n\n            <b>Core No. :</b>{{doc.core_no or ''}}<br>\n            <b>Core Location :</b>{{doc.core_location or ''}}<br>\n            <b>Structure ID :</b>{{doc.structure_id or ''}}<br>\n            <b>General Condition :</b>{{doc.general_condition or ''}}<br><br>\n\n\n        </div>\n        <div class=\"col-xs-5\">\n            <b>Date Cored :</b>{{doc.get_formatted(\"date_cored\") or ''}}<br>\n            <b>Cored by :</b>{{doc.cored_by or ''}}<br>\n            <b>Core Diameter: </b>{{doc.core_diameter or ''}}cm<br>\n            <b>Extracted Length (Max) : </b>{{doc.extracted_length or ''}}cm\n        </div>\n        <div class=\"col-xs-12\">\n            <div align=\"left\"><b>Remarks</b></div><br>\n            <div class=\"col-xs-12\">{{doc.remarks or ''}}<br></div>\n\n        </div>\n        <div class=\"col-xs-12\">\n            <p><b>Lab Testing</b></p>\n        </div>\n<br>\n\n\n        <div class=\"col-xs-7\">\n\n            <b>Surface Condition :\n\n            </b>{{doc.surface_condition or ''}}<br>\n            <b>\n                Steel Rebar, if any :\n\n            </b>{{doc.steel_rebar or ''}}<br>\n\n        </div>\n        <div class=\"col-xs-5\">\n            <p><b>Chemical Tests</b></p>\n            <br>\n            <b> Cement Content:\n\n            </b>{{doc.cement_content or ''}}<br>\n            <b>Alkali Content:\n\n\n            </b>{{doc.alkali_content or ''}}<br>\n            <b>Sulfate Content\n                :\n\n\n            </b>{{doc.sulfate_content or ''}}<br>\n            <b>Weight of concrete :\n\n            </b>{{doc.weight_concrete or ''}}\n\n        </div>\n\n        <br>\n        <br>\n        <br>\n        <p style=\"page-break-before:always;\"></p>\n         <div class=\"row\">\n        <div class=\"col-xs-12\">\n            {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n            {% if letter_head %}\n            {{ letter_head.content }}\n\n            {% endif %}\n        </div>\n    </div>\n        <div class=\"col-xs-6\">\n            <div align=\"left\"><b>UPV Test :</b></div>\n            <br>\n            <table class=\"table table-bordered\" border=\"1\">\n                <tr>\n                    <td colspan=\"2\"><b>Path (mm)</b></td>\n                    <td colspan=\"2\"><b>Time (ms)</b></td>\n                    <td colspan=\"2\"><b>Vel (km/s)</b></td>\n                </tr>\n                <tr>\n                    <td colspan=\"2\">{{doc.path_mm or ''}} </td>\n                    <td colspan=\"2\">{{doc.time_ms or ''}}</td>\n                    <td colspan=\"2\"> {{doc.vel_kms or ''}}</td>\n                </tr>\n            </table>\n\n        </div>\n\n        <div class=\"col-xs-12\">\n            <p><b>Compressive Strength Test (ASTM C 39)</b></p><br>\n        </div>\n        <div class=\"col-xs-7\">\n\n            <b>Rate of Loading, N/s : </b>{{doc.rate_of_loading_ns or ''}}<br>\n            <b>Uncapped Length (Luc): </b>{{doc.uncapped_length or ''}}cm<br>\n            <b>Capped Length (Lc) : </b>{{doc.capped_length or ''}}cm<br>\n            <b>Diameter (d): </b>{{doc.diameter_d_cm or ''}}cm<br>\n            <b>Lc/d ratio :</b>{{doc.lcd_ratio or ''}}<br>\n            <b>Strength Correction Factor : </b>{{doc.strength_correction or ''}}<br>\n            <b>Failure Load : </b>{{doc.failure_load_kn or ''}}(kN)<br>\n            <b>Corrected Strength : </b>{{doc.corrected_mpa or ''}}(MPa)<br>\n            <b> Corrected Strength :</b>{{doc.corrected_kgcm2 or ''}} (kg/cm2)<br>\n            <b>Equivalent Cube Strength :</b>{{doc.equivalent_mpa or ''}} (MPa)<br>\n            <b>Equivalent Cube Strength :</b>{{doc.equivalent_kgcm or ''}}(kg/cm2)<br>\n\n        </div>\n\n        <div class=\"col-xs-5\">\n            <b> Dry Weight: </b>{{doc.dry_weight_g or ''}}g<br>\n            <b>Saturated Weight : </b>{{doc.saturated_weight or ''}}g<br>\n            <b>Area:</b>{{doc.area_cm2 or ''}}cm2<br>\n            <b>Volume:</b> {{doc.volume_cm3 or ''}} cm3<br>\n            <b>Dry Density : </b>{{doc.dry_density or ''}}(g/cm3)<br>\n            <b>Wet Density : </b>{{doc.wet_density or ''}}(g/cm3)<br>\n            <b>Type of Fracture :</b>{{doc.type_of_fracture or ''}}<br>\n            {% if doc.type_of_fracture == \"B\"%}\n            <center> <img src=\"/files/B.png\" width='50' height=\"100\"></center>\n\n            {% endif %}\n            {% if doc.type_of_fracture == \"A\"%}<center><img src=\"/files/A.png\" width='50' height=\"100\"></center>\n\n            {% endif %}\n            {% if doc.type_of_fracture == \"C\"%}<center><img src=\"/files/C.png\" width='50' height=\"100\"></center>\n\n            {% endif %}\n            {% if doc.type_of_fracture == \"D\"%}<center><img src=\"/files/D.png\" width='50' height=\"100\"></center>\n\n            {% endif %}\n            {% if doc.type_of_fracture == \"E\"%}<center><img src=\"/files/E.png\" width='50' height=\"100\"></center>\n\n            {% endif %}\n        </div>\n\n\n    </div>\n    <br>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <b>Note:</b>\n            {% set address = doc.note %}\n            {% if address %}\n            {% set list1 = address.split('\\n') %}\n            {% endif %}\n            {% for list in list1 %}\n            <p>{{ list }}</p>\n            {% endfor %}\n        </div>\n    </div><br>\n \n\n</div>\n<p style=\"page-break-before:always;\"></p>\n<meta name=\"pdfkit-orientation\" content=\"landscape\">\n<div class=\"container\">\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n            {% if letter_head %}\n            {{ letter_head.content }}\n\n            {% endif %}\n        </div>\n    </div>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <h3>\n                <center><b> SUMMARY OF CORE TEST RESULTS </b></center>\n            </h3>\n        </div>\n        <div class=\"col-xs-7\"> <b>Project :</b>{{doc.project_name or ''}}<br><b>Consultant\n                :</b>{{doc.consultant_name or ''}}<br><b>Location :\n            </b>{{doc.location or ''}}</div>\n        <div class=\"col-xs-5\"><b>Report Ref. :</b>{{doc.report_ref or ''}}<br><b>Test Date\n                :</b>{{doc.get_formatted(\"test_date\") or ''}}<br><b>Report Date\n                :</b>{{doc.get_formatted(\"report_date\") or ''}}<br></div>\n    </div>\n\n    <div class=\"col-xs-12\">\n\n        <table class=\"table table-bordered\" border=\"1\">\n            <tr>\n                <th rowspan=\"2\" colspan=\"1\">\n                    <center><b>S. No.</b></center>\n                </th>\n                <th rowspan=\"2\" colspan=\"1\">\n                    <center><b>Core No</b></center>\n                </th>\n                <th rowspan=\"2\" colspan=\"1\">\n                    <center><b>Core Location</b></center>\n                </th>\n                <th colspan=\"1\"><b>\n                        <center>Core Dia</center>\n                    </b></th>\n                <th colspan=\"1\"><b>\n                        <center>Core Caped</center>\n                    </b></th>\n                <th rowspan=\"2\" colspan=\"1\"><b>\n                        <center>L/D Ratio</center>\n                    </b></th>\n                <th colspan=\"1\"><b>\n                        <center>Failure</center>\n                    </b></th>\n                <th colspan=\"1\"><b>\n                        <center>Correction</center>\n                    </b></th>\n                <th colspan=\"2\"><b>\n                        <center>Corrected Strength</center>\n                    </b></th>\n                <th colspan=\"2\"><b>\n                        <center>Equivalent Cube Strength</center>\n                    </b></th>\n            </tr>\n            <tr>\n                <th colspan=\"1\"><b>\n                        <center>(cm)</center>\n                    </b></th>\n                <th colspan=\"1\"><b>\n                        <center>Length (cm)</center>\n                    </b></th>\n                <th colspan=\"1\"><b>\n                        <center>Load, kN</center>\n                    </b></th>\n                <th colspan=\"1\"><b>\n                        <center>Factor</center>\n                    </b></th>\n                <th colspan=\"1\"><b>\n                        <center>N/mm2</center>\n                    </b></th>\n                <th colspan=\"1\"><b>\n                        <center>kg/cm2</center>\n                    </b></th>\n                <th colspan=\"1\"><b>\n                        <center>N/mm2<center>\n                    </b></th>\n                <th colspan=\"1\"><b>\n                        <center>kg/cm2</center>\n                    </b></th>\n            </tr>\n            {% set plt = frappe.get_all(\"Concrete Core Drilling And Strength Test\",filters ={\"project_name\":\"SAL-ORD-2019-00014\"}) %}\n\n            {% for p in plt %}\n            {% set pl = frappe.get_doc(\"Concrete Core Drilling And Strength Test\",p) %}\n\n            {% if pl.project_name == \"SAL-ORD-2019-00014\" %}\n\n            <tr>\n                <td colspan=\"1\">\n                    <center>{{loop.index or ''}}</center>\n                </td>\n                <td colspan=\"1\">\n                    <center>{{pl.core_no or ''}}</center>\n                </td>\n                <td colspan=\"1\">\n                    <center>{{pl.core_location or ''}}</center>\n                </td>\n                <td colspan=\"1\">\n                    <center>{{pl.core_diameter or ''}}</center>\n                </td>\n                <td colspan=\"1\">\n                    <center>{{pl.capped_length or ''}}</center>\n                </td>\n                <td colspan=\"1\">\n                    <center>{{pl.lcd_ratio or ''}}</center>\n                </td>\n                <td colspan=\"1\">\n                    <center>{{pl.failure_load_kn or ''}}</center>\n                </td>\n                <td colspan=\"1\">\n                    <center>{{pl.strength_correction or ''}}</center>\n                </td>\n                <td colspan=\"1\">\n                    <center>{{pl.corrected_mpa or ''}}</center>\n                </td>\n                <td colspan=\"1\">\n                    <center>{{pl.corrected_kgcm2 or ''}}</center>\n                </td>\n                <td colspan=\"1\">\n                    <center>{{pl.equivalent_mpa or ''}}</center>\n                </td>\n                <td colspan=\"1\">\n                    <center>{{pl.equivalent_kgcm or ''}}</center>\n                </td>\n            </tr>\n            {% endif %}\n            {% endfor %}\n\n        </table>\n\n\n    </div>\n\n\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <b>Note:</b>\n            {% set address = doc.note or \"\" %}\n            {% if address %}\n            {% set list1 = address.split('\\n') %}\n            {% endif %}\n            {% for list in list1 %}\n            <p>{{ list }}</p>\n            {% endfor %}\n        </div>\n    </div>\n  \n  \n  \n<footer id=\"footer-html\">\n      <div class=\"row\">\n        <div class=\"col-xs-8\">\n\n            <b> Tested by: </b><br><br><br><br><br> Laboratory Technician\n