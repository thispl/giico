 "html": "<div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Test Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n      </div>\n  </div>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <h4><b>  <center>DETERMINATION OF MC - 70 ASPHALT SPRAY RATE </center></b></h4><br>\n        </div>\n        \n        <div class=\"row\">\n        <div class=\"col-xs-3\">\n           <div align='right'> <b>Client </b> </div>\n            </div>\n            <div class=\"col-xs-3\">\n           <div align='left'> :{{doc.customer or ''}} </div>\n            </div>\n              <div class=\"col-xs-3\">\n               <div align='right'>    <b>Test Date </b> </div>\n                   </div>\n                    <div class=\"col-xs-3\">\n                        :{{doc.get_formatted(\"date_test\") or ''}}<br>\n                   </div>\n              \n                </div>\n              <div class=\"row\"> \n               <div class=\"col-xs-3\">\n            <div align='right'><b>Project Name</b> </div>\n            </div>\n               <div class=\"col-xs-3\">\n                <div align='left'>   :{{doc.project_client or ''}} </div>\n                   </div>\n                     <div class=\"col-xs-3\">   \n                 <div align='right'>   <b>Report Date</b> </div>\n                    </div>\n                    <div class=\"col-xs-3\">   \n                    :{{doc.get_formatted(\"date_report\") or ''}}<br>\n                  </div>\n                  </div> \n                 <div class=\"row\"> \n               <div class=\"col-xs-3\">\n            <div align='right'><b>Ambient Temperature </b> </div>\n            </div>\n             <div class=\"col-xs-3\">\n            <div align='left'>:{{doc.ambient_temperature or ''}} </div>\n            </div>\n            <div class=\"col-xs-3\">\n                <div align='right'>    <b>Location </b> </div>\n                </div>\n                <div class=\"col-xs-3\">\n                    :{{doc.location or ''}}<br>\n                </div>\n                  </div>\n            <div class=\"row\"> \n               <div class=\"col-xs-3\"> \n                    <div align='right'> <b>Temperature</b> </div>\n                     </div>\n                     <div class=\"col-xs-3\">\n                   <div align='left'>  :{{doc.temp or ''}} </div>\n                     </div>\n                 <div class=\"col-xs-3\"> \n                  <div align='right'><b>Report Ref No</b> </div>\n                     </div>\n                     <div class=\"col-xs-3\">\n                     :{{doc.name  or ''}}<br>\n                     </div>\n                  \n                </div>\n        </div>\n\n            <div class=\"row\">\n              <div class=\"col-xs-12\">\n            <table class=\"table table-bordered\" border=\"1\">\n                <tr>\n                    <th colspan=\"6\"><b><center>TRIAL NO.</center></b></th>\n                    <th colspan=\"2\"> <b><center>1</center></b></th>\n                    <th colspan=\"2\"><b><center>2</center></b></th>\n                    <th colspan=\"2\"><b><center>3</center></b></th>\n                </tr>\n                <tr>\n                    <td colspan=\"6\">Size of Sheet</td>\n                    {%- for row in doc.spray_rate -%}\n                    <td colspan=\"2\"><center>{{'%0.3f' % row.size|float  or ''}}</center></td>\n                    {%- endfor -%}\n                </tr>\n                <tr>\n                    <td colspan=\"6\">Area of Sheet (m2) \t</td>\n                    {%- for row in doc.spray_rate -%}\n                    <td colspan=\"2\"><center>{{'%0.3f' % row.area|float  or ''}}</center></td>\n                    {%- endfor -%}\n                </tr>\n                 <tr>\n                    <td colspan=\"6\">Wt. of Empty Sheet (kg)\t</td>\n                    {%- for row in doc.spray_rate -%}\n                    <td colspan=\"2\"><center>{{'%0.3f' % row.wt_of_empty|float or ''}}</center></td>\n                    {%- endfor -%}\n                </tr>\n                 <tr>\n                    <td colspan=\"6\">Wt. of Sheet + MC-70 (kg)  </td>\n                    {%- for row in doc.spray_rate -%}\n                    <td colspan=\"2\"><center>{{'%0.3f' % row.wt_of_sheet|float  or ''}}</center></td>\n                    {%- endfor -%}\n                </tr>\n                 <tr>\n                    <td colspan=\"6\">Wt. of MC-70 (kg)  </td>\n                    {%- for row in doc.spray_rate -%}\n                    <td colspan=\"2\"><center>{{'%0.3f' % row.wt_of_mc|float  or ''}}</center></td>\n                    {%- endfor -%}\n                </tr>\n                 <tr>\n                    <td colspan=\"6\">Rate of Spray (kg/m2)    </td>\n                    {%- for row in doc.spray_rate -%}\n                    <td colspan=\"2\"><center>{{'%0.3f' % row.rate_of_spray|float or ''}}</center></td>\n                    {%- endfor -%}\n                </tr>\n            </table>\n            <div align=\"left\"><b>Remarks</b></div>\n            <div class=\"col-xs-12\"><center>{{doc.remarks or ''}}</center></div>\n\n<div class=\"row\">\n\n        <div class=\"col-xs-2\">\n\n            <div align=\"right\"><b> Collected by</b></div>\n\n            </div>\n\n             <div class=\"col-xs-4\">\n\n            <div align=\"left\">:{{doc.collected_by or ''}}</div>\n\n            </div>\n\n            <div class=\"col-xs-3\">\n\n       \n\n             <div align=\"right\"><b> Tested By</b></div>\n\n        </div>\n\n        <div class=\"col-xs-3\">\n\n            :{{doc.tested_by or ''}}<br>\n\n        </div>\n\n    </div>\n\n    <div class=\"row\">\n\n     <div class=\"col-xs-2\">\n <div align=\"right\">{{doc.data_23}}</div>\n            </div>\n\n            </div>\n            <footer id=\"foote