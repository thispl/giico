 "html": "<meta name=\"pdfkit-orientation\" content=\"Landscape\" />\n<div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n   \n        \n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n  \n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n          <h2><b><center><u>THERMAL RESISTIVITY TEST RESULT\t</u></center></b></h2>  \n         \n         \n          \n  </div>            \n </div>\n <div class=\"row\">\n    <div class=\"col-xs-8\"><b>Project :</b>{{doc.project or \"\"}}<br><b>Client :</b>{{doc.customer or \"\"}} <br><b>Location</b>{{doc.location or \"\"}}<br><b>Date:</b>{{frappe.utils.formatdate(doc.date) or \"\"}}<br><b>Report Ref. :</b>\n{{doc.report_ref or \"\"}}</div>\n     <div class=\"col-xs-4\"><b>East</b>{{doc.east or \"\"}}<br><b>North</b>{{doc.north or \"\"}}<br><b>Equip. Used :</b>{{doc.equip_used or \"\"}}</div>\n      </div>\n       <h5><b><center><u>Reference STD : ASTM D5334\t\t\t\t</u></center></b></h5>\n       <div class=\"row\">\n        <div class=\"col-xs-12\">\n             <table class=\"table table-bordered\" border=\"1\">\n                 <tr>\n                     <th colspan= \"3\"><b><center>Test ID</center>\t</b></th>\n                       <th colspan=\"1\"><b><center>Depth (m)</center>\t</b></th>\n                         <th colspan=\"1\"><b><center>THERMAL CONDUCTIVITY (W/m0C)1</center></b></th>\n                           <th colspan=\"1\"><b><center>THERMAL CONDUCTIVITY (W/m0C)2</center></b></th>\n                             <th colspan=\"1\"><b><center>THERMAL CONDUCTIVITY (W/m0C)3</center></b></th>\n                           <th colspan=\"1\"><b><center>AVERAGE CONDUCTIVITY (W/m0C)</center></b></th>\n                             <th colspan=\"2\"><b><center>THERMAL RESISTIVITY (m0C/W)\t</center></b></th>\n                               <th colspan=\"2\"><b><center>Location, E,N</center></b></th>\n                         \t{%- for row in doc.lab_trt_result-%}        \n                 </tr>\n                 <tr>\n                     \n                                <td colspan=\"3\"><center>{{row.test_id or \"\"}}</center></td>\n                                <td colspan=\"1\"><center>{{row.depth_m or \"\"}}</center></td>\n                                <td colspan=\"1\"><center>{{row.thermal_1 or \"\"}}</center></td>\n                                  <td colspan=\"1\"><center>{{row.thermal_2 or \"\"}}</center></td>\n                                    <td colspan=\"1\"><center>{{row.thermal_3 or \"\"}}</center></td>\n                                <td colspan=\"1\"><center>{{row.average_con or \"\"}}</center></td>\n                             \n                                 <td colspan=\"2\"><center>{{row.thermal_res or \"\"}}</center></td>\n                                  <td colspan=\"2\"><center>{{row.location_en or \"\"}}</center></td>\n                                \n            </tr>\t{%- endfor -%}\n                     \n                      </table>\n                 </div>\n                 </div>\n                 <div class=\"row\">\n        \n              <div class=\"col-xs-9\"><b>Tested by:</b><br></div>\n               <div class=\"col-xs-3\"><b>Checked By:</b><br></div>\n             </div>\n            \n                 \n \n</div>",
