 "html": "<div class=\"container\">\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n        <h2><center><b>GULF INSPECTION INT'L CO.,</b></center></h2>\n        <h2><center><b> ELECTRICAL RESISTIVITY\t\t\t</b></center></h2>\n        <h4><center><b>By Wenner -4 pin method\t\t\t</b></center></h4>\n        <h4><center><b>ASTM - G - 57\t</b></center></h4>\n        <br><br>\n        <div class=\"row\">\n           <div class=\"col-xs-6\"><b>DATE TESTED: {{frappe.utils.formatdate(doc.date_tested)}}   </b><b><br>LOCATION      :{{doc.location}}</b></div>\n          <div class=\"col-xs-6\"><b>TEST NO.      :{{doc.test_no}}</b><br><b>JOB NO.</b>      :{{doc.job_no}}</div>\n          <br><br>\n          <table class=\"table table-bordered\" border=\"1\">\n         <tr>\n            \n             <th rowspan =\"2\"  colspan=\"3\">Test No.</th>\n      <th colspan=\"3\">ELECTRODE INTERVAL</th>\n      <th colspan=\"3\">RESISTANCE</th> \n      <th colspan =\"3\" >SOIL RESISTIVITY</th>\n</tr>\n\t <tr>\n\t    \n\t     <th colspan=\"3\">m</th>\n\t      <th colspan=\"3\">OHM</th>\n\t       <th colspan=\"3\">ohm-m</th>\n\t{%- for row in doc.fer_test-%}\n\t</tr>\n\t<tr>\n\t                           <td colspan=\"3\">{{row.test_no}}</td>\n                                <td colspan=\"3\">{{ row.electrode_interval}}</td>\n                                <td colspan=\"3\">{{ row.resistance}}</td>\n                                <td colspan=\"3\">{{ row.soil_resistivity}}</td>\n                                \n            </tr>\n            {%- endfor -%}\n\t\n         </table>\n         </div>\n         \n         <hr>\n         \n         </div>\n         <br><div class=\"row\">\n        <div class=\"col-xs-12\">\n          <table class=\"table table-bordered\" border=\"1\">\n        <tr>\n             <td colspan=\"12\"><center><b>Corrosivity Classification:</b></center>\t</td>\n            </tr>\n            <tr>\n              <td  colspan=\"6\"><b>Range (Ohm-m)</b></td