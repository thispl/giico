 "html": "<meta name=\"pdfkit-orientation\" content=\"Landscape\"/>\n<div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n     <div class=\"row\">\n    <div class=\"col-xs-12\">\n        <h3><center><b>MECHANICAL TEST REPORTS (ASTM A615 / A370 - E8)\t</b></center></h3><br>\n     </div>\n     <div class=\"col-xs-7\">\n         <b>Client:</b>{{doc.customer}}<br>\n         \n          <b>Project  : </b>{{doc.project or \"\"}}<br>\n           <b>Description : </b>{{doc.description or \"\"}}<br>\n            <b>Material : </b>{{doc.material or \"\"}}<br><br>\n            \n      </div>\n      <div class=\"col-xs-5\">\n           <b>Sample in Lab Date :</b>{{frappe.utils.formatdate(doc.sample_date)or \"\"}}<br>\n            <b>Date of Test :</b>{{frappe.utils.formatdate(doc.date_test)or \"\"}}<br>\n             <b>Giico. RFI. No. :</b>{{doc.giico or \"\"}}<br>\n              <b>Thick / Dia : </b>{{doc.thick or \"\"}}mm<br>\n              <b>Lab Temp.  : </b>{{doc.lab_temp or \"\"}}oC<br><br><br>\n      </div>\n      <div class=\"col-xs-12\">\n          <table class=\"table table-bordered\" border=\"1\">\n              <tr>\n                  <th colspan=\"12\"><b><center>TENSION TEST \t</center></b></th>\n              </tr>\n              <tr>\n                  <th rowspan=\"2\" colspan=\"4\"><b>Specimen ID</b></th> \n                  <th  colspan=\"1\"><b>Dimension in mm</b></th>\n                    <th   colspan=\"1\"><b>Area</b></th>\n                    <th  colspan=\"1\"><b>Ultimate Load </b></th>\n                    <th colspan=\"2\"><b>Tensile Strength\t</b></th>\n                     <th  colspan=\"1\"><b>Yield Load</b></th>\n                    <th  colspan=\"2\"><b>Yield Strength\t</b></th>\n                  \n                  \n\n              </tr>\n              <tr>\n                  <th colspan=\"1\"><b>Dia</b></th>\n                   <th colspan=\"1\"><b>mm2</b></th>\n                    <th colspan=\"1\"><b>KN </b></th>\n                     <th colspan=\"1\"><b>N/mm2</b></th>\n                     <th colspan=\"1\"><b>Ksi</b></th>\n                     <th colspan=\"1\"><b>KN </b></th>\n                     <th colspan=\"1\"><b>N/mm2</b></th>\n                     <th colspan=\"1\"><b>Ksi</b></th>\n                     {%- for row in doc.tension_test -%}\n              </tr>\n              <tr>\n                  <td colspan=\"4\">{{row.id or \"\"}}</td>\n                  <td colspan=\"1\">{{row.mm_dia or \"\"}}</td>\n                  <td colspan=\"1\">{{row.area or \"\"}}</td>\n                    <td colspan=\"1\">{{row.ultimate_load or \"\"}}</td>\n                      <td colspan=\"1\">{{row.strength or \"\"}}</td>\n                        <td colspan=\"1\">{{row.strength_ksi or \"\"}}</td>\n                          <td colspan=\"1\">{{row.yield_load_kn or \"\"}}</td>\n                            <td colspan=\"1\">{{row.yield_strength or \"\"}}</td>\n                              <td colspan=\"1\">{{row.yield_strength_ksi or \"\"}}</td>\n              </tr>\n              {%- endfor -%}\n              \n              <tr>\n                  <th colspan=\"3\"><b>Initial Gauge Length (mm) \t</b></th>\n                    <th colspan=\"3\"><b>Final Gauge Length (mm) \t</b></th>\n                      <th colspan=\"3\"><b>Elongation %\t</b></th>\n                        <th colspan=\"3\"><b>Fracture Location \t\t</b></th>\n                      \n                    \n              </tr>\n              <tr>\n                  <td colspan=\"3\">{{doc.initial_gauge or \"\"}}</td>\n                   <td colspan=\"3\">{{doc.final_gauge or \"\"}}</td>\n                    <td colspan=\"3\">{{doc.elongation or \"\"}}</td>\n                     <td colspan=\"3\">{{doc.fracture or \"\"}}</td>\n              </tr>\n              <tr>\n                  <th colspan=\"12\"><b><center>BEND TEST </center></b></th>\n              </tr>\n              <tr>\n                  <th colspan=\"2\"><b>Specimen ID</b></th>\n                  <th colspan=\"2\"><b>Type</b></th>\n                  <th colspan=\"2\"><b>Dia in mm\t</b></th>\n                  <th colspan=\"2\"><b>Bend Angle \t</b></th>\n                  <th colspan=\"2\"><b>Result\t</b></th>\n                  <th colspan=\"2\"><b>Plunger Dia </b></th>\n                  {%- for row in doc.bend_test -%}\n              </tr>\n              <tr>\n                  <td colspan=\"2\">{{row.specimen_id or \"\"}}</td>\n                  <td colspan=\"2\">{{row.type or \"\"}}</td>\n                  <td colspan=\"2\">{{row.dia_in_mm or \"\"}}</td>\n                  <td colspan=\"2\">{{row.bend_angle or \"\"}}</td>\n                  <td colspan=\"2\">{{row.result or \"\"}}</td>\n                  <td colspan=\"2\">{{row.plunger_dia or \"\"}}</td>\n              </tr>\n              {%- endfor -%}\n              <tr>\n                  <td colspan=\"12\"><b>Asper A 615 Requirements : Yield Strength ( Min : 420 mpa ) Tensile Strength ( Min : 620 mpa ) Elongation : (9 % min)\t\t</b></td>\n              </tr>\n          </table>\n          \n      </div>\n     </div>\n     <div align=\"left\"><b>Remarks</b></div>\n             <div class=\"col-xs-12\">{{doc.remarks or \"\"}}</div>\n           \n           <br><br><br><br>\n           <div class=\"row\">\n    <div class=\"col-xs-8\">Viewed by<br><br><br>QA/QC Engineer\t\t\t</div>\n    \n     <div class=\"col-xs-4\">Approved by<br><br><br>Laboratory Manager\t\n</div>\n</div>\n     </div>",
