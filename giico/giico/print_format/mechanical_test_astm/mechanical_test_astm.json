 "html": "<meta name=\"pdfkit-orientation\" content=\"Portrait\"/>\n<div class=\"container\">\n   <div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Test Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n      </div>\n  </div>\n     <div class=\"row\">\n    <div class=\"col-xs-12\">\n        <h3><center><b>MECHANICAL TEST REPORTS (ASTM A615 / A370 - E8)\t</b></center></h3><br>\n     </div>\n     \n      <div class=\"row\">\n        <div class=\"col-xs-2\">\n            <div align='right'> <b>Client</b></div>\n            </div>\n            <div class=\"col-xs-4\">\n             <div align='left'>:{{doc.customer}}</div>\n            </div>\n               <div class=\"col-xs-3\"> \n                     <div align='right'> <b>Sample in Lab Date </b></div>\n                     </div>\n                     <div class=\"col-xs-3\">\n                     <div align='left'> :{{doc.get_formatted(\"sample_date\")or \"\"}}</div>\n                     </div> \n                </div>\n              <div class=\"row\"> \n               <div class=\"col-xs-2\">\n            <div align='right'> <b>Project Name</b></div>\n            </div>\n               <div class=\"col-xs-4\">\n                 <div align='left'>   :{{doc.project_name or ''}}</div>\n                   </div>\n                    <div class=\"col-xs-3\">\n                   <div align='right'> <b>Test Date</b></div>\n                   </div>\n                    <div class=\"col-xs-3\">\n                        :{{doc.get_formatted(\"date_test\")}}<br>\n                   </div>\n                  </div> \n                  \n                 <div class=\"row\"> \n              <div class=\"col-xs-2\">\n                 <div align='right'>    <b>Material </b></div>\n                </div>\n                <div class=\"col-xs-4\">\n                  :{{doc.material or \"\"}}<br>\n                </div>\n             