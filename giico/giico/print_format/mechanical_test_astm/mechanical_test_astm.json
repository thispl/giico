 "html": "<meta name=\"pdfkit-orientation\" content=\"Landscape\"/>\n<div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n     <div class=\"row\">\n    <div class=\"col-xs-12\">\n        <h3><center><b>MECHANICAL TEST REPORTS (ASTM A615 / A370 - E8)\t</b></center></h3><br>\n     </div>\n     <div class=\"col-xs-7\">\n         <b>Client:</b>{{doc.customer}}<br>\n         \n          <b>Project  : </b>{{doc.project or \"\"}}<br>\n           <b>Description : </b>{{doc.description or \"\"}}<br>\n            <b>Material : </b>{{doc.material or \"\"}}<br>\n             <b>Lab Temp.  : </b>{{doc.lab_temp or \"\"}}oC<br>\n      </div>\n      <div class=\"col-xs-5\">\n           <b>Sample in Lab Date :</b>{{frappe.utils.formatdate(doc.sample_date)or \"\"}}<br>\n            <b>Date of Test :</b>{{frappe.utils.formatdate(doc.date_test)or \"\"}}<br>\n             <b>Giico. RFI. No. :</b>{{doc.giico or \"\"}}<br>\n              <b>Thick / Dia : </b>{{doc.thick or \"\"}}mm<br>\n             \n      </div>\n      <div class=\"col-xs-12\">\n          <table class=\"table table-bordered\" border=\"1\">\n              <tr>\n                  <th colspan=\"12\"><b><center>TENSION TEST \t</center></b></th>\n              </tr>\n              <tr>\n                  <th rowspan=\"2\" colspan=\"4\"><center><b>Specimen ID</b></center></th> \n                  <th  colspan=\"1\"><center><b>Dimension in mm</b></center></th>\n                    <th   colspan=\"1\"><center><b>Area</b></center></th>\n                    <th  colspan=\"1\"><center><b>Ultimate Load </b></center></th>\n                    <th colspan=\"2\"><center><b>Tensile Strength\t</b></center></th>\n                     <th  colspan=\"1\"><center><b>Yield Load</b></center></th>\n                    <th  colspan=\"2\"><center><b>Yield Strength\t</b></center></th>\n                  \n                  \n\n              </tr>\n              <tr>\n                  <th colspan=\"1\"><center><b>Dia</b></center></th>\n                   <th colspan=\"1\"><center><b>mm2</b></center></th>\n                    <th colspan=\"1\"><center><b>KN </b></center></th>\n                     <th colspan=\"1\"><center><b>N/mm2</b></center></th>\n                     <th colspan=\"1\"><center><b>Ksi</b></center></th>\n                     <th colspan=\"1\"><center><b>KN </b></center></th>\n                     <th colspan=\"1\"><center><b>N/mm2</b></center></th>\n                     <th colspan=\"1\"><center><b>Ksi</b></center></th>\n                     {%- for row in doc.tension_test -%}\n              </tr>\n              <tr>\n                  <td colspan=\"4\"><center>{{row.id or \"\"}}</center></td>\n                  <td colspan=\"1\"><center>{{row.mm_dia or \"\"}}</center></td>\n                  <td colspan=\"1\"><center>{{row.area or \"\"}}</center></td>\n                    <td colspan=\"1\"><center>{{row.ultimate_load or \"\"}}</center></td>\n                      <td colspan=\"1\"><center>{{row.strength or \"\"}}</center></td>\n                        <td colspan=\"1\"><center>{{row.strength_ksi or \"\"}}</center></td>\n                          <td colspan=\"1\"><center>{{row.yield_load_kn or \"\"}}</center></td>\n                            <td colspan=\"1\"><center>{{row.yield_strength or \"\"}}</center></td>\n                              <td colspan=\"1\"><center>{{row.yield_strength_ksi or \"\"}}</center></td>\n              </tr>\n              {%- endfor -%}\n              \n              <tr>\n                  <th colspan=\"3\"><b><center>Initial Gauge Length (mm) </center>\t</b></th>\n                    <th colspan=\"3\"><b><center>Final Gauge Length (mm) </center>\t</b></th>\n                      <th colspan=\"3\"><b><center>Elongation %</center>\t</b></th>\n                        <th colspan=\"3\"><b><center>Fracture Location </center>\t\t</b></th>\n                      \n                    \n              </tr>\n              <tr>\n                  <td colspan=\"3\"><center>{{doc.initial_gauge or \"\"}}</center></td>\n                   <td colspan=\"3\"><center>{{doc.final_gauge or \"\"}}</center></td>\n                    <td colspan=\"3\"><center>{{doc.elongation or \"\"}}</center></td>\n                     <td colspan=\"3\"><center>{{doc.fracture or \"\"}}</center></td>\n              </tr>\n              <tr>\n                  <th colspan=\"12\"><b><center>BEND TEST </center></b></th>\n              </tr>\n              <tr>\n                  <th colspan=\"2\"><center><b>Specimen ID</b></center></th>\n                  <th colspan=\"2\"><center><b>Type</b></center></th>\n                  <th colspan=\"2\"><center><b>Dia in mm\t</b></center></th>\n                  <th colspan=\"2\"><center><b>Bend Angle \t</b></center></th>\n                  <th colspan=\"2\"><center><b>Result\t</b></center></th>\n                  <th colspan=\"2\"><center><b>Plunger Dia </b></center></th>\n                  {%- for row in doc.bend_test -%}\n              </tr>\n              <tr>\n                  <td colspan=\"2\"><center>{{row.specimen_id or \"\"}}</center></td>\n                  <td colspan=\"2\"><center>{{row.type or \"\"}}</center></td>\n                  <td colspan=\"2\"><center>{{row.dia_in_mm or \"\"}}</center></td>\n                  <td colspan=\"2\"><center>{{row.bend_angle or \"\"}}</center></td>\n                  <td colspan=\"2\"><center>{{row.result or \"\"}}</center></td>\n                  <td colspan=\"2\"><center>{{row.plunger_dia or \"\"}}</center></td>\n              </tr>\n              {%- endfor -%}\n              <tr>\n                  <td colspan=\"12\"><b><center>Asper A 615 Requirements : Yield Strength ( Min : 420 mpa ) Tensile Strength ( Min : 620 mpa ) Elongation : (9 % min)\t</center>\t</b></td>\n              </tr>\n          </table>\n          \n      </div>\n     </div>\n     <div align=\"left\"><b>Remarks</b></div>\n             <div class=\"col-xs-12\">{{doc.remarks or \"\"}}</div>\n           \n           <br><br><br><br>\n           <div class=\"row\">\n    <div class=\"col-xs-8\"><b>Viewed by</b><br><br><br>QA/QC Engineer\t\t\t</div>\n    \n     <div class=\"col-xs-4\"><b>Approved by</b><br><br><br>Laboratory Manager\t\n</div>\n</div>\n     </div>",
