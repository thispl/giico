 "html": "<meta name=\"pdfkit-orientation\" content=\"Landscape\" />\n<div class =\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n  \n  \n  \n    <div class=\"row\">\n        <h4><b><center>LABORATORY ELECTRIC RESISTIVITY TEST (ASTM G187)\t\t\t\t\t\t\t\t\n</center></b></h4>\n        <div class=\"col-xs-7\"><b>Date of Test :</b>{{frappe.utils.formatdate(doc.date_of_test) or \"\"}}</div>\n        <div class=\"col-xs-5\"><b>\tRoom Temp. (co) :</b>{{doc.room_temp_co or \"\"}}<sup>0</sup>C</div>\n    </div>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <table class =\"table table-bordered\" border=\"1\">\n                <tr>\n                  <th colspan=\"1\"><b><center>Test No.</center></b></th>\n                   <th colspan=\"1\"><b><center>Sample Source (BH)</center></b></th>\n                    <th colspan=\"1\"><b><center>Depth (m)</center></b></th>\n                     <th colspan=\"1\"><b><center>Sample Weight (gms)</center></b></th>\n                      <th colspan=\"1\"><b><center>Wet Density (mg/m3)</center></b></th>\n                       <th colspan=\"1\"><b><center>Moisture Content (%)</center></b></th>\n                        <th colspan=\"2\"><b><center>Dry  Density (mg/m3)</center></b></th>\n                         <th colspan=\"2\"><b><center>Soil Resistivity (W-CM)</center></b></th>\n                          <th colspan=\"2\"><b><center>Soil Description</center></b></th>\n                  {%- for row in doc.lab_ert_test -%}\n                  \n                </tr>\n                <tr>\n                    <td colspan=\"1\"><center>{{row.test_no or \"\"}}</center></td>\n                    <td colspan=\"1\"><center>{{row.sam_so_bh or \"\"}}</center></td>\n                    <td colspan=\"1\"><center>{{row.depth_m or \"\"}}</center></td>\n                     <td colspan=\"1\"><center>{{row.sw_gms or \"\"}}</center></td>\n                      <td colspan=\"1\"><center>{{row.wet_den_mgm3 or \"\"}}</center></td>\n                       <td colspan=\"1\"><center>{{row.moisture_con or \"\"}}</center></td>\n                        <td colspan=\"2\"><center>{{row.dry_den or \"\"}}</center></td>\n                         <td colspan=\"2\"><center>{{row.soil_res or \"\"}}</center></td>\n                          <td colspan=\"2\"><center>{{row.soil_des or \"\"}}</center></td>\n                </tr>\n                {%- endfor -%}\n            </table>\n        </div>\n    </div>\n    <div class=\"row\">\n        <div class=\"col-xs-6\"><b>Equipment used:\t</b><br>{{doc.equipment_used or \"\"}}</div>\n        <div class=\"col-xs-6\"><b>\tTest Condition:\t\t</b><br>{{doc.test_condition or \"\"}}</div>\n        <br><br><br><br><br><br>\n          <div class=\"col-xs-6\">Tested by :<br><br><br></div>\n        <div class=\"col-xs-6\">Checked by :<br><br><br></div>\n        \n    </div>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n           \n            <b>Project:</b>{{doc.project or \"\"}}<br>\n             <b>Client:</b>{{doc.customer or \"\"}}<br> \n                   <b>Location:</b>{{doc.location or \"\"}}\n                \n          \n          \n        </div>\n          \n    \n    \n   </div> \n</div>",
