rder=\"1\">\n                <tr>\n                    <th colspan=\"3\"><b>Description:</b></th>\n                      <th colspan=\"3\"><b>Marshall Rammer</b></th>\n                        <th colspan=\"3\"><b>Stander Limit</b></th>\n                          <th colspan=\"3\"><b>Remak</b></th>\n                          {%- for row in doc.marshall_rammer -%}\n                </tr>\n                <tr>\n                    <td colspan=\"3\"><center>{{row.description}}</center></td>\n                    <td colspan=\"3\"><center>{{row.marshall}}</center></td>\n                    <td colspan=\"3\"><center>{{row.stander}}</center></td>\n                    <td colspan=\"3\"><center>{{row.remak}}</center></td>\n                </tr>\n                {%-endfor-%}\n            </table>\n        </div>\n    </div>\n    \n    \n    \n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n   <b>Remarks</b>\n             \n             {% set address = doc.remarks %}\n                    {% if address %}\n                    {% set list1 = address.split('\\n') %}\n                    {% endif %}\n                    {% for list in list1 %}\n                    <p>{{ list or ''}}</p>\n                    {% endfor %}</div>\n           </div>\n           </div>\n           <div class=\"row\">\n\n        <div class=\"col-xs-2\">\n\n            <div align=\"right\"><b> Collected by</b></div>\n\n            </div>\n\n             <div class=\"col-xs-4\">\n\n            <div align=\"left\">:{{doc.collected_by or ''}}</div>\n\n            </div>\n\n            <div class=\"col-xs-3\">\n\n       \n\n             <div align=\"right\"><b> Tested By</b></div>\n\n        </div>\n\n        <div class=\"col-xs-3\">\n\n            :{{doc.tested_by or ''}}<br>\n\n        </div>\n\n    </div>\n\n    <div class=\"row\">\n\n     <div class=\"col-xs-2\">\n <div align=\"right\">{{doc.data_22}}</div>\n            </div>\n\n            </div>\n  \n           \n           <footer id=\"footer-html\">\n           <div class=\"row\">\n    <div class=\"col-xs-8\"><b>CALIBRATED BY</b><br><br><br><br>Sr. Lab Technican\t\t</div>\n    \n     <div class=\"col-xs-4\"><b>CHECKED BY</b><br><br><br><br>Laboratory Manager\t\n</div>\n</div>\n<div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Test Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.footer }}\n  \n  {% endif %}\n      </div>\n  </div>\n</footer>\n   </div>        \n",
