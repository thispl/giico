 "html": "<div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <h4><b><center>CRACK WIDTH MEASURMENTS ON SLAB\t</center></b></h4>\n            </div>\n            </div>\n            <div class=\"row\">\n        <div class=\"col-xs-2\">\n            <div align='right'><b>Client</b></div>\n            </div>\n            <div class=\"col-xs-4\">\n            <div align='left'>:{{doc.customer or ''}}</div>\n            </div>\n               <div class=\"col-xs-3\">\n                <div align='right'>   <b>Location</b></div>\n                </div>\n                <div class=\"col-xs-3\">\n                    :{{doc.location or ''}}<br>\n                </div>\n                </div>\n              <div class=\"row\"> \n               <div class=\"col-xs-2\">\n            <div align='right'><b>Project Name</b></div>\n            </div>\n               <div class=\"col-xs-4\">\n                 <div align='left'>  :{{doc.project_name or ''}}</div>\n                   </div>\n                    <div class=\"col-xs-3\">\n              <div align='right'><b>Report Ref</b></div>\n                   </div>\n                    <div class=\"col-xs-3\">\n                        :{{doc.name or ''}}<br>\n                   </div>\n                  </div> \n           \n            <div class=\"row\">\n             <div class=\"col-xs-12\">\n            <table class =\"table table-bordered\" bordered=\"1\">\n                <tr>\n                    <th rowspan=\"2\" colspan=\"1\"><b><center>SL NO.</center></b></th>\n                     <th rowspan=\"2\" colspan=\"1\"><center><b>AREA</b></center></th>\n                      <th rowspan=\"2\" colspan=\"1\"><center><b>CRACK ID</b></center></th>\n                       <th  colspan=\"4\"><b><center>CRACKN DIVISION </center>\t</b></th>\n                         <th rowspan=\"2\" colspan=\"1\"><center><b>DIV.DIFFERENCE</b></center></th>\n                           <th rowspan=\"2\" colspan=\"2\"><center><b>CRACK WIDTH MM</b></center></th>\n                             <th rowspan=\"2\" colspan=\"2\"><center><b>REMARKS </b></center></th>\n                </tr>\n                <tr>\n                    <th colspan=\"2\"><b><center>START</center></b></th>\n                    <th colspan=\"2\"><b><center>END</center></b></th>\n                    {%- for row in doc.crack_width -%}\n                </tr>\n                <tr>\n                    <td colspan=\"1\"><center>{{row.slno or ''}}</center></td>\n                     <td colspan=\"1\"><center>{{row.area or ''}}</center></td>\n                      <td colspan=\"1\"><center>{{loop.index or ''}}</center></td>\n                       <td colspan=\"2\"><center>{{row.start or ''}}</center></td>\n                        <td colspan=\"2\"><center>{{row.end or ''}}</center></td>\n                         <td colspan=\"1\"><center>{{row.div_differ or ''}}</center></td>\n                          <td colspan=\"2\"><center>{{row.crack_width or ''}}</center></td>\n                           <td colspan=\"2\"><center>{{row.remark or ''}}</center></td>\n                            \n                </tr>\n                {%- endfor -%}\n            </table>\n        </div></div>\n          <div align=\"left\"><b>Note</b></div>\n             <div class=\"col-xs-12\">\n                 {% set address = doc.note %}\n                 {% if address %}\n                 {% set list1= address.split(\"\\n\") %}\n                 {% endif %}\n                 {% for list in list1 %}\n                 <p>{{list or ''}}</p>\n                 {% endfor %}\n             </div>\n           \n           <br><br><br><br><footer id=\"footer-html\">\n           <div class=\"row\">\n    <div class=\"col-xs-8\"><br><br><br><br>Field Tech<br> \n\t</div>\n    \n     <div class=\"col-xs-4\"><br><br><br><br>Lab Manager\t</div>\n</div></footer>\n    \n</div>",
