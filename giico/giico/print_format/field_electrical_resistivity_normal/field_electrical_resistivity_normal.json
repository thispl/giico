ss=\"row\">\n        <div class=\"col-xs-12\">\n          <table class=\"table table-bordered\" border=\"1\">\n         <tr>\n             <td colspan=\"12\"><center><b>Corrosivity Classification:</b></center>\t</td>\n            </tr>\n            <tr>\n              <td  colspan=\"6\"><b>Range (Ohm-m)</b></td>\n              <td colspan=\"6\"><b>Potential</b></td>\n              </tr>\n              <tr>\n                  <td colspan=\"6\"><10</td>\n                   <td colspan=\"6\">Very Severly corrosive</td>\n              </tr>\n               <tr>\n                  <td colspan=\"6\">10 to 20</td>\n                   <td colspan=\"6\">Severly corrosive</td>\n              </tr> <tr>\n                  <td colspan=\"6\">20 to 50</td>\n                   <td colspan=\"6\">Moderately corrosive</td>\n              </tr>\n               <tr>\n                  <td colspan=\"6\">>50</td>\n                   <td colspan=\"6\">Mildly corrosive</td>\n              </tr>\n            </table>\n            </div>\n            </div>\n              </div>\n              </div>\n              <br>\n              <div class=\"col-xs-8\"><b>Tested by:</b><br>{{doc.tested_by}}<br><br><b>Field/Lab Technician\t</b></div>\n          <div class=\"col-xs-4\"><b>Checked by:</b><br>{{doc.checked_by}}</div>\n           <br>\n           <br>\n              \n           <table class=\"table table-bordered\" border=\"1\">\n          <tr>\n              <td colspan=\"12\"> <b>PROJECT :</b>{{doc.project}}</td>\n          </tr><tr>\n              <td colspan=\"12\"><b>CLIENT:</b>{{doc.client}}</td>\n          </tr>\n          <tr>\n              <td colspan=\"12\"><b>LOCATION :</b>{{doc.location}}</td>\n          </tr>\n    </table>\n</div>\n      </div>\n    </div>\n</div>",
