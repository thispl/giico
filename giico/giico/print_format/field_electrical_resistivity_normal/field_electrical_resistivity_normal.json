 "html": "<div class=\"container\">\n     <div class=\"row\">\n        <div class=\"col-xs-12\">\n     {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n      <h4><u><center>ELECTRICAL RESISTIVITY TEST<br>Wenne 4-pin Method (ASTM - G - 57)\t</center>\t</u></h4> \n        </div>\n        </div>\n    \n         <div class=\"row\">\n           <div class=\"col-xs-7\"><b>Date :{{frappe.utils.formatdate(doc.date)or \"\"}}</b><br><b>GIICo Ref No :{{doc.giico_ref_no or \"\"}}</b><br><b>Test No. :{{ doc.test_no or \"\"}}</b></div>\n          <div class=\"col-xs-5\"><b>EAST:</b>{{doc.east or \"\"}}<br><b>NORTH:</b>{{doc.north or \"\"}}<br><b>Elevation :{{doc.elevation or \"\"}}</b>      </div>\n           </div>\n           <div class=\"row\">\n        <div class=\"col-xs-12\">\n          <table class=\"table table-bordered\" border=\"1\">\n              <tr>\n                  <th colspan=\"3\"><center>TEST NO.</center></th>\n                    <th colspan=\"3\"><center>ELECTRODE INTERVAL</center></th>\n                      <th colspan=\"3\"><center>RESISTANCE</center></th>\n                        <th colspan=\"3\"><center>SOIL RESISTIVITY</center></th>\n              </tr>\n               <tr>\n                  <th colspan=\"3\"><center>0</center></th>\n                    <th colspan=\"3\"><center>M</center></th>\n                      <th colspan=\"3\"><center>OHM</center></th>\n                        <th colspan=\"3\"><center>OHM.M</center></th>\n                        \t{%- for row in doc.table_7-%}\n              </tr>\n              \t<tr>\n\t                           <td colspan=\"3\"><center>{{row.test_no or \"\"}}</center></td>\n                                <td colspan=\"3\"><center>{{ row.electrode_interval or \"\"}}</center></td>\n                                <td colspan=\"3\"><center>{{ row.resistance or \"\"}}</center></td>\n                                <td colspan=\"3\"><center>{{ row.soil_resistivity or \"\"}}</center></td>\n                                \n            </tr>\n            {%- endfor -%}\n\t\n              \n              </table>\n              </div>\n              </div>\n              <div class=\"row\">\n        <div class=\"col-xs-12\">\n          <table class=\"table table-bordered\" border=\"1\">\n         <tr>\n             <td colspan=\"12\"><center><b>Corrosivity Classification:</b></center>\t</td>\n            </tr>\n            <tr>\n              <td  colspan=\"6\"><b>Range (Ohm-m)</b></td>\n              <td colspan=\"6\"><b>Potential</b></td>\n              </tr>\n              <tr>\n                  <td colspan=\"6\">10</td>\n                   <td colspan=\"6\">Very Severly corrosive</td>\n              </tr>\n               <tr>\n                  <td colspan=\"6\">10 to 20</td>\n                   <td colspan=\"6\">Severly corrosive</td>\n              </tr> <tr>\n                  <td colspan=\"6\">20 to 50</td>\n                   <td colspan=\"6\">Moderately corrosive</td>\n              </tr>\n               <tr>\n                  <td colspan=\"6\">>50</td>\n                   <td colspan=\"6\">Mildly corrosive</td>\n              </tr>\n            </table>\n            </div>\n            </div>\n             \n              <br>\n              <div class=\"col-xs-8\"><b>Tested by:</b><br><br><br><br><b>Field/Lab Technician\t</b><br><br><br>   <br></div>\n          <div class=\"col-xs-4\"><b>Checked by:</b><br><br><br>   <br>\n           <br></div>\n           <br>\n           <br>\n              <div class=\"row\">\n                  <div class=\"col-xs-8\">\n           \n          \n               <b>PROJECT :</b>{{doc.project or \"\"}}<br>\n          \n              <b>CLIENT:</b>{{doc.customer or \"\"}}<br>\n\n              <b>LOCATION :</b>{{doc.location or \"\"}}\n         \n\n</div>