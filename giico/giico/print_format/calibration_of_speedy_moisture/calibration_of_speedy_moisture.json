 "html": "<div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n    \n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <h2><center><b><u>Calibration Of Speedy Moisture(ASTM D-4944)</u></b></center></h2><br>\n                 \n                  <div class=\"row\"> \n                  <div class=\"col-xs-2\">\n                  <div align='right'><b>Client</b></div>\n                   </div>\n               <div class=\"col-xs-4\">\n                <div align='left'>   :{{doc.customer or ''}}</div>\n                  </div>\n              \n                    <div class=\"col-xs-3\">\n                   <div align='right'><b>Calibration Date</b></div>\n                   </div>\n                    <div class=\"col-xs-3\">\n                        :{{frappe.utils.formatdate(doc.calibration_date) or ''}}<br>\n                   </div>\n                  </div>\n            <div class=\"row\"> \n             <div class=\"col-xs-2\">\n            <div align='right'><b>Project Name</b></div>\n            </div>\n               <div class=\"col-xs-4\">\n                <div align='left'>   :{{doc.project_name or ''}}</div>\n                   </div>\n              \n                     <div class=\"col-xs-3\"> \n                    <div align='right'> <b>Name</b></div>\n                     </div>\n                     <div class=\"col-xs-3\">\n                     :{{doc.name1 or ''}}<br>\n                     </div>\n                </div>\n                 <div class=\"row\"> \n                  <div class=\"col-xs-2\">\n               <div align='right'> <b>Serial No</b></div>\n                </div>\n                <div class=\"col-xs-4\">\n               <div align='left'> :{{doc.serial_no or ''}}</div>\n                 </div>\n                     <div class=\"col-xs-3\"> \n                    <div align='right'> <b> Report Ref No</b></div>\n                     </div>\n                     <div class=\"col-xs-3\">\n                    <div align='left'> :{{doc.name or ''}}</div>\n                     </div>\n                </div>\n                </div>\n                 <div class=\"col-xs-8\">\n                 <table class=\"table table-bordered\" border=\"1\">\n                     <tr>\n                         <th colspan=\"8\"><b><center>Moisture Content (%)\t</center></b></th>\n                     </tr>\n                     <tr>\n                         <th colspan=\"2\"><b>Oven Drying</b></th>\n                           <th colspan=\"2\"><b>Speedy Moisture</b></th>\n                             <th colspan=\"2\"><b>Difference</b></th>\n                               <th colspan=\"2\"><b>% Difference</b></th>\n                               {%- for row in doc.moisture_content -%}\n                     </tr>\n                     <tr>\n                         <td colspan=\"2\">{{row.oven_drying or ''}}</td>\n                           <td colspan=\"2\">{{row.speedy or 0}}</td>\n                             <td colspan=\"2\">{{row.differen or 0}}</td>\n                               <td colspan=\"2\">{{row.differen1 or 0}}</td>\n                     </tr>\n                     {%- endfor-%}\n                 </table><br>\n    </div>\n    </div>\n    \n    <!--<canvas id=\"chart_image\" width=\"10\" height=\"100\"></canvas>-->\n<!--<p id=\"caption\">The chart is displaying a simple line chart.</p>-->\n\n     <div align=\"left\"><b>Remarks</b></div>\n             <div class=\"col-xs-12\">{{doc.remarks or ''}}</div>\n             \n           <!--<div id=\"chart\">{{doc.chat_image}}</div>-->\n           <br><br><br><br><footer id=\"footer-html\">\n           <div class=\"row\">\n    <div class=\"col-xs-8\"><b>CALIBRATED BY</b><br><br><br>Sr. Lab Technican\t\t</div>\n    \n     <div class=\"c