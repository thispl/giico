 "html": "<div class=\"container\">\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <h2><center><b><u>Calibration Of Speedy Moisture(ASTM D-4944)</u></b></center></h2><br><br>\n                 <div class=\"col-xs-12\"><b>CALIBRATION DATE:</b>{{frappe.utils.formatdate(doc.calibration_date)}}<br><b>SERIAL NO:</b>{{doc.serial_no}}<br><b>NAME:</b>{{doc.name}}<br><b>GIICo REPORT REF:</b>{{doc.report_ref}}</div><br>\n                </div>\n                 <div class=\"col-xs-8\">\n                 <table class=\"table table-bordered\" border=\"1\">\n                     <tr>\n                         <td colspan=\"8\"><b><center>Moisture Content (%)\t</center></b></td>\n                     </tr>\n                     <tr>\n                         <td colspan=\"2\"><b>Oven Drying</b></td>\n                           <td colspan=\"2\"><b>Speedy Moisture</b></td>\n                             <td colspan=\"2\"><b>Difference</b></td>\n                               <td colspan=\"2\"><b>% Difference</b></td>\n                               {%- for row in doc.moisture_content -%}\n                     </tr>\n                     <tr>\n                         <td colspan=\"2\">{{row.oven_drying}}</td>\n                           <td colspan=\"2\">{{row.speedy}}</td>\n                             <td colspan=\"2\">{{row.differen}}</td>\n                               <td colspan=\"2\">{{row.differen1}}</td>\n                     </tr>\n                     {%- endfor-%}\n                 </table><br>\n    </div>\n    </div>\n     <div align=\"left\"><b>Remarks</b></div>\n             <div class=\"col-xs-12\">{{doc.remarks}}</div>\n           \n           <br><br><br><br>\n           <div class=\"row\">\n    <div class=\"col-xs-8\"><b>CALIBRATED BY</b><br><br><br>Sr. Lab Technican\t\t</div>\n    \n     <div class=\"col-xs-4\"><b>CHECKED BY</b><br><br><br>Laboratory Manager<BR><BR><BR>\t\n</div><br>\n<div class=\"col-xs-8\"><b>PROJECT:</b>{{doc.projec