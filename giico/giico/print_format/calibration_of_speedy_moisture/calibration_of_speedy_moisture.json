 "html": "<div class=\"container\">\r\n\t\t\t<div class=\"row\">\r\n        <div class=\"col-xs-12\">\r\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Test Logo\") %}\r\n  {% if letter_head %}\r\n\t{{ letter_head.content }}\r\n  \r\n  {% endif %}\r\n      </div>\r\n  </div>\r\n  \r\n    <div class=\"row\">\r\n        <div class=\"col-xs-12\">\r\n            <h2>\r\n                <center><b><u>Calibration Of Speedy Moisture(ASTM D-4944)</u></b></center>\r\n            </h2><br>\r\n\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-2\">\r\n                    <div align='right'><b>Client</b></div>\r\n                </div>\r\n                <div class=\"col-xs-4\">\r\n                    <div align='left'> :{{doc.customer or ''}}</div>\r\n                </div>\r\n\r\n                <div class=\"col-xs-3\">\r\n                    <div align='right'><b>Calibration Date</b></div>\r\n                </div>\r\n                <div class=\"col-xs-3\">\r\n                    :{{ doc.calibration_date.strftime('%d-%m-%Y') or ''}}<br>\r\n                </div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-2\">\r\n                    <div align='right'><b>Project Name</b></div>\r\n                </div>\r\n                <div class=\"col-xs-4\">\r\n                    <div align='left'> :{{doc.project_client or ''}}</div>\r\n                </div>\r\n\r\n                <div class=\"col-xs-3\">\r\n                    <div align='right'> <b>Name</b></div>\r\n                </div>\r\n                <div class=\"col-xs-3\">\r\n                    :{{doc.name1 or ''}}<br>\r\n                </div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <div class=\"col-xs-2\">\r\n                    <div align='right'> <b>Serial No</b></div>\r\n                </div>\r\n                <div class=\"col-xs-4\">\r\n                    <div align='left'> :{{doc.serial_no or '