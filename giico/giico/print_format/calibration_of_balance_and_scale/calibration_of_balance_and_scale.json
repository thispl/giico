 "html": "<meta name=\"pdfkit-orientation\" content=\"Landscape\"/>\n<div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n        <h3><b><center>REPORT ON CALIBRATION OF BALANCE & SCALE <br>(ASTM - D 4753)\t\t</center></b></h3>\n<br>\n        </div>\n        <div class=\"col-xs-7\">\n            <b>CLIENT NAME :</b>{{doc.customer or ''}}<br>\n                <b>PROJECT NAME:</b>{{doc.project or ''}}<br>\n                    <b>MAKE/BRAND : </b>{{doc.make or ''}}<br>\n                        <b>CAPACITY :</b>{{doc.capacity or ''}}kg<br>\n                            <b>\tEQUIPMENT ID/S.N:</b>{{doc.equipment_id or ''}}<br>\n        \n        </div>\n         <div class=\"col-xs-5\">\n            <b>READABILITY :</b>{{doc.readability or ''}}g<br>\n                <b>CALIBRATION DATE:</b>{{frappe.utils.formatdate(doc.calibration_date) or ''}}<br>\n                    <b>REPORT DATE:</b>{{frappe.utils.formatdate(doc.report_date) or ''}}<br>\n                        <b>REPORT REF No:</b>{{doc.report_ref_no or ''}}<br>\n                            \n        \n        </div>\n        <div class=\"col-xs-12\">\n            <table class=\"table table-bordered\" border=\"1\">\n                <tr>\n                    <th  rowspan=\"2\" colspan=\"2\"><b>SR.No</b></th>\n                    <th  rowspan=\"2\" colspan=\"2\"><b>STANDARD WEIGHT (g)</b></th>\n                    <th rowspan=\"2\" colspan=\"2\"><b>BALANCE WEIGHT(g)</b></th>\n                    <th  colspan=\"4\"><b>DIFFERENCE\t</b></th>\n                    <th  rowspan=\"2\" colspan=\"2\"><b>REMARKS</b></th>\n                </tr>\n                <tr>\n                    <th colspan=\"2\"><b>(g)</b></th>\n                     <th colspan=\"2\"><b>(%)</b></th>\n                     {%- for row in doc.balance -%}\n                </tr>\n                <tr>\n                    <td colspan=\"2\">{{loop.index or ''}}</td>\n                       <td colspan=\"2\">{{row.standard_weight or ''}}</td>\n                          <td colspan=\"2\">{{row.balance_weight or ''}}</td>\n                             <td colspan=\"2\">{{row.differenceg or ''}}</td>\n                                <td colspan=\"2\">{{row.difference or ''}}</td>\n                                   <td colspan=\"2\">{{row.remarks or ''}}</td>\n                </tr>\n                {%- endfor -%}\n            </table>\n        </div>\n        \n        </div>\n         <div align=\"left\"><b>Remarks</b></div>\n             <div class=\"col-xs-12\">\n              {% set address= doc.remarks %}\n              {% if address %}\n              {% set list1 = address.split(\"\\n\") %} \n              {% endif %}\n              {% for list in list1 %}\n              <p>{{list}}</p>\n                 {% endfor %}\n                 \n             </div>\n           \n           <br><br><br><br><br>\n           <div class=\"row\">\n    <div class=\"col-xs-8\"><b>CALIBRATED BY</b><br><br><br><BR>\tLab. Technician</div>\n    \n     <div class=\"col-xs-4\"><b>CHECKED BY</b><br><br><br><BR>Laboratory Manager\t</div>\n</div>\n           \n           \n        </div>",
