                </div>\n               <div class=\"col-xs-3\"> \n                     <div align='right'><b>Report Ref.No</b></div>\n                     </div>\n                     <div class=\"col-xs-3\">\n                     <div align='left'>:{{doc.name or ''}}</div>\n                     </div>\n                     \n                  </div>\n            <div class=\"row\"> \n               <div class=\"col-xs-2\"> \n                    <div align='right'> <b>Cored by</b></div>\n                     </div>\n                     <div class=\"col-xs-4\">\n                    <div align='left'> :{{doc.cored_by or \"\"}}</div>\n                     </div>\n                     <div class=\"col-xs-3\"> \n                    <div align='right'><b>Date of Core </b></div>\n                     </div>\n                     <div class=\"col-xs-3\">\n                     :{{doc.get_formatted(\"date_of_core\")or \"\"}}<br>\n                     </div>\n                 \n    </div>\n         <div class=\"col-xs-12\">\n             <table classs=\"table table-bordered\" border=\"1\">\n                 <tr>\n                     <th colspan=\"1\"><b><center>Core Location & Sample No.</center></b></th>\n                     <th colspan=\"1\"><b><center>Type of Asphalt</center></b></th>\n                      <th colspan=\"1\"><b><center>Mass of Sample in Air (g)</center></b></th>\n                       <th colspan=\"1\"><b><center>Mass of Sample in Water (g)</center></b></th>\n                        <th colspan=\"1\"><b><center>Mass of SSD Sample (g)</center></b></th>\n                         <th colspan=\"1\"><b><center>Calculated Bulk Density (g/cm\u00b3)</center></b></th>\n                          <th colspan=\"1\"><b><center>Job Mix Density (g/cm\u00b3)</center></b></th>\n                           <th colspan=\"1\"><b><center>Percent Compaction %</center></b></th>\n                            <th colspan=\"1\"><b><center>Average Thickness (mm)</center></b></th>\n                              <th colspan=\"1\"><b><center>Total Thickness(mm)</center></b></th>\n                               <th colspan=\"2\"><b><center>Average thickness (mm)</center></b></th>\n                               {%- for row in doc.bulk_specific -%}\n                 </tr>\n                 <tr>\n                     <td colspan=\"1\"><center>{{row.core_location or \"\"}}</center></td>\n                      <td colspan=\"1\"><center>{{row.type_asphalt or \"\"}}</center></td>\n                         <td colspan=\"1\"><center>{{row.mass_of_sample or \"\"}}</center></td>\n                            <td colspan=\"1\"><center>{{row.mass_of_water or \"\"}}</center></td>\n                               <td colspan=\"1\"><center>{{row.mass_of_ssd or \"\"}}</center></td>\n                                  <td colspan=\"1\"><center>{{'%0.4f' % row.calculated_bulk|float or \"\"}}</center></td>\n                                     <td colspan=\"1\"><center>{{'%0.4f' % row.job_mix |float or \"\"}}</center></td>\n                                        <td colspan=\"1\"><center>{{row.percent_com or \"\"}}</center></td>\n                                           <td colspan=\"1\"><center>{{row.aver_thick or \"\"}}</center></td>\n                                              <td colspan=\"1\"><center>{{row.total_thickness or \"\"}}</center></td>\n                                                 <td colspan=\"2\"><center>{{row.aver_thick1 or \"\"}}</center></td>\n                 </tr>\n                 {%- endfor -%}\n             </table>\n             <br><br>\n             </div>\n              <div align=\"left\"><b>Remarks</b></div>\n             <div class=\"col-xs-12\">\n              {% set address= doc.remarks or \"\" %}\n              {% if address %}\n              {% set list1 = address.split(\"\\n\") %} \n              {% endif %}\n              {% for list in list1 %}\n              <p>{{list}}</p>\n                 {% endfor %}\n              