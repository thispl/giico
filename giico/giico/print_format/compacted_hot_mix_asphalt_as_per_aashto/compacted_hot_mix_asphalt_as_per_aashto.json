 "html": "<meta name=\"pdfkit-orientation\" content=\"Landscape\"/>\n<div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <h4><b><center>BULK SPECIFIC GRAVITY OF COMPACTED HOT MIX ASPHALT<br>  AS PER AASHTO T-230\t</center></b></h4><br>\n        \n        </div>\n        <div class=\"col-xs-7\">\n           <b>Project Name :</b>{{doc.project or \"\"}}<br> \n           <b>Contractor :</b>{{doc.contractor or \"\"}}<br> \n           <b>Location :</b>{{doc.location or \"\"}}<br>\n           <b>Cored by :</b>{{doc.cored_by or \"\"}}<br><br>\n              </div>\n        <div class=\"col-xs-5\">\n           <b>Date of Test :</b>{{frappe.utils.formatdate(doc.date_of_test)or \"\"}}<br> \n           <b>\tDate of Report :</b>{{frappe.utils.formatdate(doc.date_of_report)or \"\"}}<br> \n            <b>Report Ref :</b>{{doc.report_ref or \"\"}}<br> \n           <b>Date of Core :</b>{{frappe.utils.formatdate(doc.date_of_core)or \"\"}}<br> <br>\n            \n        </div>\n         <div class=\"col-xs-12\">\n             <table classs=\"table table-bordered\" border=\"1\">\n                 <tr>\n                     <th colspan=\"1\"><b><center>Core Location & Sample No.</center></b></th>\n                     <th colspan=\"1\"><b><center>Type of Asphalt</center></b></th>\n                      <th colspan=\"1\"><b><center>Mass of Sample in Air (g)</center></b></th>\n                       <th colspan=\"1\"><b><center>Mass of Sample in Water (g)</center></b></th>\n                        <th colspan=\"1\"><b><center>Mass of SSD Sample (g)</center></b></th>\n                         <th colspan=\"1\"><b><center>Calculated Bulk Density (g/cm3)</center></b></th>\n                          <th colspan=\"1\"><b><center>Job Mix Density (g/cm3)</center></b></th>\n                           <th colspan=\"1\"><b><center>Percent Compaction %</center></b></th>\n                            <th colspan=\"1\"><b><center>Average Thickness (mm)</center></b></th>\n                              <th colspan=\"1\"><b><center>Total Thickness(mm)</center></b></th>\n                               <th colspan=\"2\"><b><center>Average thickness (mm)</center></b></th>\n                               {%- for row in doc.bulk_specific -%}\n                 </tr>\n                 <tr>\n                     <td colspan=\"1\"><center>{{row.core_location or \"\"}}</center></td>\n                      <td colspan=\"1\"><center>{{row.type_asphalt or \"\"}}</center></td>\n                         <td colspan=\"1\"><center>{{row.mass_of_sample or \"\"}}</center></td>\n                            <td colspan=\"1\"><center>{{row.mass_of_water or \"\"}}</center></td>\n                               <td colspan=\"1\"><center>{{row.mass_of_ssd or \"\"}}</center></td>\n                                  <td colspan=\"1\"><center>{{row.calculated_bulk or \"\"}}</center></td>\n                                     <td colspan=\"1\"><center>{{row.job_mix or \"\"}}</center></td>\n                                        <td colspan=\"1\"><center>{{row.percent_com or \"\"}}</center></td>\n                                           <td colspan=\"1\"><center>{{row.aver_thick or \"\"}}</center></td>\n                                              <td colspan=\"1\"><center>{{row.total_thickness or \"\"}}</center></td>\n                                                 <td colspan=\"2\"><center>{{row.aver_thick1 or \"\"}}</center></td>\n                 </tr>\n                 {%- endfor -%}\n             </table>\n             <br><br>\n             </div>\n              <div align=\"left\"><b>Remarks</b></div>\n             <div class=\"col-xs-12\">\n              {% set address= doc.remarks or \"\" %}\n              {% if address %}\n              {% set list1 = address.split(\"\\n\") %} \n              {% endif %}\n              {% for list in list1 %}\n              <p>{{list}}</p>\n                 {% endfor %}\n                 \n             </div>\n           \n           <br><br><br><br><br>\n           <div class=\"row\">\n    <div class=\"col-xs-8\"><b>CALIBRATED BY</b><br><br><br><BR>\tLab. Technician</div>\n    \n     <div class=\"col-xs-4\"><b>CHECKED BY</b><br><br><br><BR>Laboratory Manager\t</div>\n</div>\n           \n           \n        \n    </div>\n</div>",
