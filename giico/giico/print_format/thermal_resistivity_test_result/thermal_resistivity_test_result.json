 "html": "<div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-xs-12\">\n  {% set letter_head = frappe.get_doc(\"Letter Head\",\"Giico Logo\") %}\n  {% if letter_head %}\n\t{{ letter_head.content }}\n  \n  {% endif %}\n  </div>\n  </div>\n  \n    <div class=\"row\">\n        <div class=\"col-xs-12\">\n            <h4><center><b><u>THERMAL RESISTIVITY TESTS RESULTS\t</u></b></center></h4><br>\n        </div>\n        \n       <div class=\"row\">\n        <div class=\"col-xs-2\">\n           <div align='right'>  <b>Client</b></div>\n            </div>\n            <div class=\"col-xs-4\">\n            <div align='left'> :{{doc.customer}}</div>\n            </div>\n\t\t\t <div class=\"col-xs-3\">   \n                  <div align='right'>   <b>Report Date</b></div>\n                    </div>\n                    <div class=\"col-xs-3\">   \n                    :{{frappe.utils.formatdate(doc.date_report) or ''}}<br>\n              \n                </div>\n\t\t\t\t</div>\n              <div class=\"row\"> \n               <div class=\"col-xs-2\">\n           <div align='right'>  <b>Project Name</b></div>\n            </div>\n               <div class=\"col-xs-4\">\n                <div align='left'>    :{{doc.project_name or ''}}</div>\n                   </div>\n                    <div class=\"col-xs-3\">\n                   <div align='right'> <b>Test Date</b></div>\n                   </div>\n                    <div class=\"col-xs-3\">\n                        :{{frappe.utils.formatdate(doc.date_test)}}<br>\n                   </div>\n                  </div> \n                  \n                 <div class=\"row\"> \n\t\t\t\t     \n               <div class=\"col-xs-2\">\n                  <div align='right'>   <b>Ref STD</b></div>\n                </div>\n                <div class=\"col-xs-4\">\n                   <div align='left'>  :{{doc.ref_std or \"\"}}</div>\n                </div>\n               <div class=\"col-xs-3\"> \n                  <div align='right'>    <b>Report Ref No</b></div>\n                     </div>\n                     <div class=\"col-xs-3\">\n                     :{{doc.name or ''}}<br>\n                     </div>\n                  \n                  </div>\n            <div class=\"row\"> \n               <div class=\"col-xs-2\">\n           <div align='right'> <b>Air Temperature</b></div>\n            </div>\n               <div class=\"col-xs-4\">\n                 <div align='left'>   :{{doc.air_temp or \"\"}}\u00b0C</div>\n                   </div>\n                    <div class=\"col-xs-3\">\n                 <div align='right'>    <b>Sample type </b></div>\n                   </div>\n                    <div class=\"col-xs-3\">\n                        :{{doc.sample_type or \"\"}}<br>\n                   </div>\n                  </div> \n                  \n                 <div class=\"row\"> \n               <div class=\"col-xs-2\"> \n                    <div align='right'>  <b>Test Condition</b></div>\n                     </div>\n                     <div class=\"col-xs-4\">\n                    <div align='left'>  :{{doc.test_con or \"\"}}</div>\n                     </div>\n                   <div class=\"col-xs-3\">   \n                    <div align='right'>  <b>Equip used:</b></div>\n                    </div>\n                    <div class=\"col-xs-3\">   \n                    :{{doc.equip_used or \"\"}}<br>\n                  </div>\n                  </div>\n            <div class=\"col-xs=12\">\n                <table class=\"table table-bordered\" border=\"1\">\n                    <tr>\n                        <th colspan=\"2\"><b><center>SAMPLE ID No.</center> </b></th>\n                         <th colspan=\"3\"><b><center>SOIL DESCRIPTING </center></b></th>\n                          <th colspan=\"1\"><b><center>Temp. of Soil </center></b></th>\n                           <th colspan=\"2\"><b><center>MOISTURE (%)</center>\t</b></th>\n                            <th colspan=\"2\"><b><center>THERMAL CONDUCTIVITY (W/mk)</center></b></th>\n                             <th colspan=\"2\"><b><center>THERMAL RESISTIVITY (mk/W)</center></b></th>\n                             {%- for row in doc.resistivity_test -%}\n                    </tr>\n                    <tr>\n                         <td colspan=\"2\"><center>{{loop.index}}</center></td>\n                        <td colspan=\"3\"><center>{{row.soil_des or \"\"}}</center></td>\n                           <td colspan=\"1\"><center>{{row.temp_soil or \"\"}}</center></td>\n                              <td colspan=\"2\"><center>{{row.moisture or \"\"}}</center></td>\n                                 <td colspan=\"2\"><center>{{row.thermal1 or \"\"}}</center></td>\n                                    <td colspan=\"2\"><center>{{row.thermal2 or \"\"}}</center></td>\n                                      \n                    </tr>\n                    {%- endfor -%}\n                </table>\n            </div>\n    </div>\n     <div align=\"left\"><b>Remarks</b></div>\n             <div class=\"col-xs-12\">\n                 {% set address = doc.remarks or \"\"%}\n                 {% if address %}\n                 {% set list1 = address.split(\"\\n\") %}\n                 {% endif %}\n                 {% for list in list1 %}\n                 <p>{{list}}</p>\n                 {% endfor %}\n             </div>\n           \n           <br><br><br><br>\n           <footer id=\"footer-html\">\n\n           <div class=\"row\">\n    <div class=\"col-xs-8\"><b>Tested by :</b><br><br><br> <br>Chemist\n\t\n\t</div>\n    \n     <div class=\"col-xs-4\"><b>Approved by :</b><br><br><br><br>Laboratory Manager\t</div>\n</div></footer>\n</div>",
